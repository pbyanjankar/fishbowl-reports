		<queryString>
			<![CDATA[SELECT post.id as postid, 1 as Sort,  part.num as Part,  coalesce(inventorylog.CHANGEQTY,post.quantity) AS Qty,uom.code AS Uomcode, ABS(post.amount) AS amount, post.datecreated AS datecreated, post.dateposted AS dateposted,
    post.refitemid AS type, inventorylog.info as Notes,

coalesce(INVENTORYACCOUNT.accountNumber,
   coalesce((case part.typeid
		when 10 then defaultInventoryAccount.accountNumber
		when 50 then defaultCapitalEquipment.accountNumber
 		else 'Improper Part Type'
	end),'Default Inventory Account') ) as ACCOUNTNUMBER,

    coalesce(INVENTORYACCOUNT.name,coalesce(
(case part.typeid when 10 then defaultInventoryAccount.name when 50 then defaultCapitalEquipment.name
 else 'Improper Part Type' end),'Default Inventory Account') ) as Account,

-- (case post.refitemid when 2 then coalesce(ScrapAccount.name ,defaultScrappedInventory.name) else coalesce (INVENTORYACCOUNT.name, defaultInventoryAdjustment.name) end) as "Account",

 (case  post.refitemid when 2 then 0.00 else (case when  post.amount > 0 then   post.amount else 0.00 end ) end) as Debit,
  (case  post.refitemid when 2 then post.amount else (case when  post.amount < 0 then   post.amount * (-1) else 0.00 end ) end) as Credit,
begloc.name as beglocation,
endloc.name as endlocation,
lg.name as lg

FROM post
    INNER JOIN part ON post.refid = part.id
    INNER JOIN uom ON uom.id = part.uomid
    INNER JOIN company ON company.id = 1
	left join inventorylog on (post.id = inventorylog.recordid and post.refid = inventorylog.PARTID and inventorylog.tableid = -1407594912)
   left join location begloc on begloc.id = inventorylog.beglocationid
   left join location endloc on endloc.id = inventorylog.endlocationid
   left join locationgroup lg on lg.id = inventorylog.locationgroupid


 left join asaccount INVENTORYACCOUNT on INVENTORYACCOUNT.id = part.INVENTORYACCOUNTID

left join (select asaccount.name, asaccount.accountNumber from asaccount inner join sysproperties on (sysproperties.syskey = 'Acct ID inventoryAsset' and sysproperties.sysvalue = asaccount.id)) defaultInventoryAccount on 1 = 1
left join (select asaccount.name, asaccount.accountNumber from asaccount inner join sysproperties on (sysproperties.syskey = 'Acct ID Capital Equipment' and sysproperties.sysvalue = asaccount.id)) defaultCapitalEquipment on 1 = 1

WHERE post.typeid = 50
and UPPER(part.num) LIKE UPPER($P{partNum})
     AND (post.dateposted BETWEEN $P{dateRange1DatePosted} AND $P{dateRange2DatePosted} or post.dateposted is null )
     AND (post.datecreated BETWEEN $P{dateRange1TransactionDate} AND $P{dateRange2TransactionDate})
 AND post.refitemid IN ($P!{ckAddInitialInv},$P!{ckScrap},$P!{ckCycleCount},$P!{ckCostChange})

union all
SELECT  post.id as postid, 2 as Sort, part.num as Part,  coalesce(inventorylog.CHANGEQTY,post.quantity) AS Qty,uom.code AS Uomcode, ABS(post.amount) AS amount, post.datecreated AS datecreated, post.dateposted AS dateposted,
    post.refitemid AS type, inventorylog.info as Notes,

(case post.refitemid when 2 then coalesce(ScrapAccount.accountNumber ,defaultScrappedInventory.accountNumber) else coalesce (ADJUSTMENTACCOUNT.accountNumber, defaultInventoryAdjustment.accountNumber) end) as ACCOUNTNUMBER,

   -- coalesce(INVENTORYACCOUNT.name,coalesce( (case part.typeid when 10 then defaultInventoryAccount.name when 50 then defaultCapitalEquipment.name  else 'Improper Part Type' end),'Default Inventory Account') ) as Account,

 (case post.refitemid when 2 then coalesce(ScrapAccount.name ,defaultScrappedInventory.name) else coalesce (ADJUSTMENTACCOUNT.name, defaultInventoryAdjustment.name) end) as "Account",

  (case  post.refitemid when 2 then post.amount else (case when  post.amount < 0 then   post.amount * (-1) else 0.00 end ) end) as Debit,
   (case  post.refitemid when 2 then 0.00 else (case when  post.amount > 0 then   post.amount else 0.00 end ) end) as Credit,
begloc.name as beglocation,
endloc.name as endlocation,
lg.name as lg

FROM post
    INNER JOIN part ON post.refid = part.id
    INNER JOIN uom ON uom.id = part.uomid
    INNER JOIN company ON company.id = 1
	left join inventorylog on (post.id = inventorylog.recordid and post.refid = inventorylog.PARTID and inventorylog.tableid = -1407594912)

 left join asaccount ADJUSTMENTACCOUNT on ADJUSTMENTACCOUNT.id = part.ADJUSTMENTACCOUNTID
 left join asaccount ScrapAccount on ScrapAccount.id = part.SCRAPACCOUNTID


left join (select asaccount.name, asaccount.accountNumber from asaccount inner join sysproperties on (sysproperties.syskey = 'Acct ID Inventory Adjustment' and sysproperties.sysvalue = asaccount.id)) defaultInventoryAdjustment on 1 = 1
left join (select asaccount.name, asaccount.accountNumber from asaccount inner join sysproperties on (sysproperties.syskey = 'Acct ID Scrapped Inventory' and sysproperties.sysvalue = asaccount.id)) defaultScrappedInventory on 1 = 1
 left join location begloc on begloc.id = inventorylog.beglocationid
 left join location endloc on endloc.id = inventorylog.endlocationid
 left join locationgroup lg on lg.id = inventorylog.locationgroupid



WHERE post.typeid = 50
and UPPER(part.num) LIKE UPPER($P{partNum})
     AND (post.dateposted BETWEEN $P{dateRange1DatePosted} AND $P{dateRange2DatePosted} or post.dateposted is null )
     AND (post.datecreated BETWEEN $P{dateRange1TransactionDate} AND $P{dateRange2TransactionDate})
 AND post.refitemid IN ($P!{ckAddInitialInv},$P!{ckScrap},$P!{ckCycleCount},$P!{ckCostChange})

order by 1 desc, 2]]>
