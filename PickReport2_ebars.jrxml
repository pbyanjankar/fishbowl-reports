	<queryString>
		<![CDATA[SELECT PI1.ID AS PICKID, PI1.NUM AS PICKNUMBER, PICKTYPE.NAME AS PICKTYPE, PICKSTATUS.NAME AS PICKSTATUS,
    PRIORITY.NAME AS PICKPRIORITY, PICKLG.NAME AS PICKLOCATIONGROUP, PICKLG.ID AS PICKLOCATIONGROUPID,
    PI1.DATECREATED AS PICKDATECREATED, PI1.DATESCHEDULED AS PICKDATESCHEDULED, PI1.DATEFINISHED AS PICKDATEFINISHED,
    PI1.DATELASTMODIFIED AS PICKDATELASTMODIFIED, PI1.DATESTARTED AS PICKDATESTARTED,
    PICKITEM.ID AS PICKITEMID, PICKITEMSTATUS.NAME AS PICKITEMSTATUS, PICKITEMSTATUS.ID AS PICKITEMSTATUSID,
    PART.NUM AS PICKITEMPARTNUM, PART.DESCRIPTION AS PICKITEMPARTDESC, round(PICKITEM.QTY,1) AS PICKITEMQTY, UOM.CODE AS PICKITEMUOM,
 MAINTAG.NUM AS PICKITEMTAGNUM,
    PICKITEM.SLOTNUM AS PICKITEMSLOTNUM, COMPANY.NAME AS COMPANY,
    (CASE
	WHEN PICKITEM.ORDERTYPEID = 10 THEN POITEM.NOTE
	WHEN PICKITEM.ORDERTYPEID = 20 THEN SOITEM.NOTE
	WHEN PICKITEM.ORDERTYPEID = 40 THEN XOITEM.NOTE
    END) AS PICKITEMNOTE, COALESCE(SOITEM.DESCRIPTION,PART.DESCRIPTION) AS SOITEMDESCRIPTION,
    kitHeader.productNum AS kitHeaderName, kitHeader.description AS kitHeaderDescription, kitHeader.id AS kitFlag,
     lot.info AS LOTNUMBER,
     expiration.info AS EXPIRATION,
     internallot.info AS INTERNALLOTNUMBER


FROM pick PI1
    INNER JOIN picktype ON PI1.typeid = picktype.id
    INNER JOIN pickstatus ON PI1.statusid = pickstatus.id
    INNER JOIN priority ON PI1.priority = priority.id
    INNER JOIN locationgroup picklg ON PI1.locationgroupid = picklg.id
    LEFT OUTER JOIN pickitem ON PI1.id = pickitem.pickid
    LEFT OUTER JOIN tag maintag ON COALESCE(pickitem.srctagid,pickitem.tagid) = maintag.id
    LEFT OUTER JOIN pickitemstatus ON pickitem.statusid = pickitemstatus.id
    LEFT OUTER JOIN part ON pickitem.partid = part.id
    LEFT OUTER JOIN uom ON pickitem.uomid = uom.id
    LEFT OUTER JOIN tag desttag ON pickitem.desttagid = desttag.id
    LEFT OUTER JOIN poitem ON (pickitem.poitemid = poitem.id AND pickitem.ordertypeid = 10)
    LEFT OUTER JOIN soitem ON (pickitem.soitemid = soitem.id AND pickitem.ordertypeid = 20)
    LEFT OUTER JOIN woitem ON (pickitem.woitemid = woitem.id AND pickitem.ordertypeid = 30)
    LEFT OUTER JOIN xoitem ON (pickitem.xoitemid = xoitem.id AND pickitem.ordertypeid = 40)
    LEFT OUTER JOIN so ON soitem.soid = so.id
    JOIN company ON company.id = 1
    LEFT OUTER JOIN objectToObject AS obo ON obo.recordid1 = soitem.id AND obo.typeid = 30
    LEFT OUTER JOIN soitem AS kitHeader ON kitHeader.id = obo.recordid2 AND kitheader.typeid = 80

LEFT JOIN trackingtext AS lot ON lot.tagId = pickitem.tagId AND lot.partTrackingId = (SELECT id FROM parttracking where name='Lot Number')

LEFT JOIN trackingDate AS expiration ON expiration.tagId = pickitem.tagId AND expiration.partTrackingId = (SELECT id FROM parttracking where name='Expiration Date')

LEFT JOIN trackingtext AS internallot ON internallot.tagId = pickitem.tagId AND internallot.partTrackingId = (SELECT id FROM parttracking where name='Internal Lot Number')

WHERE (PI1.num = $P{pickNum})

ORDER BY IF($P!{ShowKitHeaders},COALESCE(kitheader.id,"ZZ"), part.num), part.num
]]>
	</queryString>
