		<queryString>
			<![CDATA[SELECT
so.num AS SONUMBER,
so.customerPO AS CUSTOMERPO,
so.dateCreated,
so.dateFirstShip,
product.num AS PRODUCTNUMBER,
product.description AS PRODUCTDESCRIPTION,
soitem.qtyOrdered AS QTYORDERED,
customer.name AS CUSTOMER,
(soItem.qtyOrdered - soItem.qtyFulfilled) AS QTYREMAINING,
backorder.totalRequired AS TOTALREQUIRED,
transfer.transferQTY AS TRANSFERQTY,
    LOCATIONGROUP.NAME AS LG,
    COALESCE(QTYONHAND.QTY, 0) AS QOH
FROM (SELECT part.id AS partId, sum(xoitem.qtyToFulfill) AS transferQTY
FROM xo
INNER JOIN xoitem ON xo.id = xoitem.xoid
INNER JOIN part ON part.id = xoitem.partid
WHERE (xo.num =  $P{toNum} OR xo.num =  $P{toNumTONumber2} OR xo.num =  $P{toNumTONumber3}) AND xo.statusId < 70
GROUP BY part.id) AS transfer
INNER JOIN part ON part.id = transfer.partId
INNER JOIN product ON product.partid = part.id
INNER JOIN soitem ON soitem.productid = product.id AND soitem.statusId < 50
INNER JOIN so ON so.id = soitem.soid AND so.statusId IN (10,20,25)
INNER JOIN customer ON customer.id = so.customerId
INNER JOIN (SELECT part.id AS partId, SUM(soItem.qtyOrdered - soItem.qtyFulfilled) totalRequired
 	FROM soitem
	INNER JOIN so ON soitem.soid = so.id AND so.statusId IN (10,20,25)
	INNER JOIN product ON product.id = soitem.productid
          INNER JOIN part ON part.id = product.partid
          INNER JOIN xoitem ON xoitem.partId = part.id
	INNER JOIN xo ON xo.id = xoitem.xoid
	WHERE (xo.num =  $P{toNum} OR xo.num =  $P{toNumTONumber2} OR xo.num =  $P{toNumTONumber3}) AND xo.statusId < 70 AND soitem.statusId < 50
          GROUP BY part.id
) AS backOrder ON backOrder.partid = part.id
LEFT JOIN locationgroup ON locationgroup.id IN ($P!{locationGroupID})
LEFT JOIN qtyonhand ON qtyonhand.partid = part.id AND qtyonhand.locationgroupid = locationgroup.id

WHERE
 customer.name LIKE concat($P{Customer},'%')
ORDER BY product.num, $P!{cbOrderBy} $P!{cbSort}, $P!{cbThenOrderBy_1} $P!{cbSort_1}, $P!{cbThenOrderBy_2} $P!{cbSort_2}]]>
		</queryString>
