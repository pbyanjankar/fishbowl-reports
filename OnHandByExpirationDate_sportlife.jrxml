		<queryString>
			<![CDATA[SELECT PART.NUM AS PARTNUM, TAG.PARTID AS PARTID, TAG.LOCATIONID AS LOCATIONID, TAG.TRACKINGENCODING AS TRACKINGENCODING,
    PART.DESCRIPTION AS PARTDESC, PART.SERIALIZEDFLAG, UOM.CODE AS PARTUOM, TAGQTY,
    COALESCE(LOCATION.NAME, '') AS LOCATION, COALESCE(LOCATIONGROUP.NAME, '') AS LOCATIONGROUP,
    TAGTRACKINGVIEW.INFOFORMATTED AS EXPDATE,
    (CASE
       WHEN timestampdiff(day, current_date, cast(tagtrackingview.infoFormatted AS date)) > 30 THEN 'Valid'
       WHEN timestampdiff(day, current_date, cast(tagtrackingview.infoFormatted AS date)) <= 30
        AND timestampdiff(day, current_date, cast(tagtrackingview.infoFormatted AS date)) > 0 THEN 'Expiring'
       ELSE 'Expired'
       END) AS STATUS, timestampdiff(day, current_date, cast(tagtrackingview.infoFormatted AS date)) AS DATEEXPIRING

FROM part
    LEFT JOIN uom ON part.uomid = uom.id
    INNER JOIN (SELECT MAX(id) AS id, COALESCE(SUM(qty), 0) AS tagQty, partID, locationID, trackingEncoding
          FROM Tag
          GROUP BY trackingEncoding, partID, locationID) as Tag ON part.id = tag.partid
    LEFT JOIN location ON tag.locationid = location.id
    LEFT JOIN locationgroup ON location.locationgroupid = locationgroup.id
    LEFT JOIN tagtrackingview ON tagtrackingview.tagid = tag.id AND tagTrackingView.typeID <> 40
    LEFT OUTER JOIN vendorparts ON vendorparts.partid = part.id AND vendorparts.defaultflag = 1
    LEFT OUTER JOIN vendor on vendor.id = vendorparts.vendorId

WHERE part.typeid = 10
  AND (tagtrackingview.typeid = 30 OR tagtrackingview.typeid IS NULL)
  AND part.num LIKE $P{partNum}
  AND (location.id LIKE $P{locationID} OR location.name IS NULL)
  AND (locationgroup.id IN ($P!{locationGroupID}) OR locationgroup.id IS NULL)
  AND tagtrackingview.info BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)
  AND vendor.id LIKE $P{vendorID}

GROUP BY part.num, tag.partID, tag.locationID, tag.trackingEncoding, part.description, part.serializedflag,
    uom.code, tag.trackingEncoding, location.name, locationgroup.name, tagQty, ExpDate

HAVING (CASE WHEN $P{cbStatus} = CAST('All' AS CHAR(10))
	THEN CAST('All' AS CHAR(10))
	ELSE (CASE
                 WHEN timestampdiff(day, current_date, cast(tagtrackingview.infoFormatted AS date)) > 30 THEN CAST('Valid' AS CHAR(10))
                  WHEN timestampdiff(day, current_date, cast(tagtrackingview.infoFormatted AS date)) <= 30
                   AND timestampdiff(day, current_date, cast(tagtrackingview.infoFormatted AS date)) > 0 THEN CAST('Expiring' AS CHAR(10))
                  ELSE CAST('Expired' AS CHAR(10))
                  END)
	END) = $P{cbStatus}

ORDER BY LocationGroup, UPPER($P!{cbOrderBy}), UPPER(part.num)]]>
		</queryString>
