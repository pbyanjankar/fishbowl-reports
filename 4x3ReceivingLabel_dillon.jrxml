	<queryString>
		<![CDATA[SELECT

    RECEIPT.ID AS RECEIPTID,
    receiptitem.dateReceived AS DATERECEIVED,
    part.num AS RECEIPTITEMPARTNUM,
    PART.DESCRIPTION AS RECEIPTITEMPARTDESC,

poitem.vendorpartnum AS VENDORPARTNUMBER,
trackinginfo.info AS LOTNUMBER




FROM receipt
    INNER JOIN receiptitem ON receipt.id = receiptitem.receiptid
    INNER JOIN part ON part.id = receiptitem.partId
    INNER JOIN receipttype ON receipt.typeid = receipttype.id
    INNER JOIN receiptstatus ON receipt.statusid = receiptstatus.id
     inner join poitem on poitem.id = receiptitem.poitemid
    LEFT OUTER JOIN ordertype ON receipt.ordertypeid = ordertype.id
    LEFT OUTER JOIN trackinginfo ON trackinginfo.recordId = receiptitem.id AND tableid = 1259138490
WHERE receipt.id = $P!{receiptID}
  AND receiptitem.statusid IN (20,30,40)
  AND receipt.orderTypeId = 10;]]>
	</queryString>
