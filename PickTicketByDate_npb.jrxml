	<queryString>
		<![CDATA[SELECT
    pick.num AS PICKNUM

FROM
    pick

WHERE
    pick.statusid IN ($P{ckStarted},$P{ckCommitted})
    AND $P!{cbApplyDateFilter} BETWEEN $P{dateRange1}
        AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)

ORDER BY
    $P!{cbApplyDateFilter}, pick.num]]>
	</queryString>
