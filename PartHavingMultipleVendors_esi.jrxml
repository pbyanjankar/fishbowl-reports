	<queryString>
		<![CDATA[SELECT
    part.num AS Part,
    company.name AS company,
    count(*) AS times
FROM part
    INNER JOIN vendorparts ON part.id = vendorparts.partid
    INNER JOIN vendor ON vendor.id = vendorparts.vendorid AND vendor.activeFlag = 1
    JOIN company ON company.id = 1
WHERE part.id > 0
AND part.activeflag = 1
AND part.id not in (SELECT part.id FROM part
    INNER JOIN vendorparts ON part.id = vendorparts.partid
    INNER JOIN vendor on vendor.id = vendorparts.vendorid
    where vendor.name in ($P{ExcludePartsHavingVendors})
 )
GROUP BY 1,2
HAVING times>1
ORDER BY part.id;]]>
	</queryString>
