		<parameter name="MarkertingData" class="java.lang.String" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT customer.name AS Customer,
       customer.number AS ACCOUNTNUMBER,
       maxso.lastorderdate AS "Last Order Date",
       DATEDIFF(CURRENT_DATE, maxso.lastorderdate) AS "Days Dormant",
       so.num AS "Last SO#",
       so.totalprice AS "Last Order Amount",
       ordercount.total AS "Total Orders",
      postCatch.totalpaid AS "Total Spent",
	     company.name AS company,
	     sysuser.username AS "Sales Rep",
	     stateconst.code AS "State",
       $P!{CustomField:Customer1} AS customercf1,
customer.lastchangeduser,
accountgroup.name as accountgroup,
cast(CustomFieldByName(customer.CUSTOMFIELDS, 'Dormant Follow Up Date') as date) AS dmt,

CustomFieldByName(customer.CUSTOMFIELDS, 'Customer Type') AS CUSTOMERTYPE,
CustomFieldByName(customer.CUSTOMFIELDS, 'Dormant Follow Up Feedback') AS DORMANTFOLLOWUPFEEDBACK,
CustomFieldByName(customer.CUSTOMFIELDS, 'Next Dormant Follow Up Date') AS NEXTDORMANTFOLLOWUPDATE,

coalesce((select contact.contactname from contact
where contact.accountid=customer.accountid
and contact.typeid=50
limit 1),'') as CName,
coalesce((select contact.datus from contact
where contact.accountid=customer.accountid
and contact.typeid=60
limit 1),'') as CEmail,
coalesce((select contact.datus from contact
where contact.accountid=customer.accountid
and contact.typeid=50
limit 1),'') as CPhone,
coalesce((
SELECT SUM(amount) FROM postransaction p1
INNER JOIN so ON p1.soid=so.id
WHERE so.customerid=customer.id),0) as TotRecvd,
CustomFieldByName(customer.CUSTOMFIELDS, 'Default LG') as lg1,
CustomFieldByName(customer.CUSTOMFIELDS, 'Marketing Data') as md1,
coalesce((
select lastchangeduser from customer_aud

where id=customer.id and CAST(CustomFieldByName(customer_aud.CUSTOMFIELDS, 'Dormant Follow Up Date') AS DATE) is not null
order by rev ASC limit 1),'') ludmt,
CustomFieldByName(customer.CUSTOMFIELDS, 'Magento Email') as MagEmail,
customer.datecreated,

coalesce((SELECT COUNT(SO.id) FROM so WHERE
so.datecreated BETWEEN (CURDATE() - INTERVAL 1 MONTH ) AND CURDATE()
and so.customerid=customer.id
),0) as last30day,

coalesce((SELECT COUNT(SO.id) FROM so WHERE
so.datecreated BETWEEN (CURDATE() - INTERVAL 2 MONTH ) AND CURDATE()
and so.customerid=customer.id
),0) as last60day,

coalesce((SELECT COUNT(SO.id) FROM so WHERE
so.datecreated BETWEEN (CURDATE() - INTERVAL 3 MONTH ) AND CURDATE()
and so.customerid=customer.id
),0) as last90day,
coalesce((SELECT memo
FROM memo
WHERE tableid = 2142022944
  AND recordid = customer.id
ORDER BY id desc limit 1),'') as lastmemo,
coalesce(cTret.TOTALPRICE,0) as cTotRet

FROM customer
     LEFT OUTER JOIN accountgrouprelation ON customer.accountid = accountgrouprelation.accountid
     LEFT OUTER JOIN accountgroup ON accountgrouprelation.groupid = accountgroup.id
     LEFT OUTER JOIN sysuser ON customer.defaultsalesmanid = sysuser.id
     LEFT OUTER JOIN address ON customer.accountid = address.accountid AND address.typeid = 50 AND address.defaultflag = true
     LEFT OUTER JOIN stateconst On address.stateid = stateconst.id
     INNER JOIN company ON company.id = 1
     inner JOIN (SELECT MAX($P!{cbLastOrdered}) AS lastorderdate, so.customerid, so.id
                 FROM so
                 WHERE so.statusid IN (10,20,25,60,70)
                 GROUP BY so.customerid) AS maxso ON customer.id = maxso.customerid
  inner JOIN so ON so.customerid = customer.id
    inner JOIN (SELECT COUNT(so.id) AS total,  so.customerid
                 FROM so
                 WHERE so.statusid IN (10,20,25,60,70)
                 GROUP BY so.customerid) AS ordercount ON customer.id = ordercount.customerid

left join (
SELECT  customer.id AS cid,
    CUSTOMER.NAME AS CUSTOMERNAME,  SUM(SOITEM.TOTALPRICE + (SOITEM.ADJUSTAMOUNT / (CASE WHEN SOITEM.QTYTOFULFILL = 0
                                                            THEN 1
                                                            ELSE SOITEM.QTYTOFULFILL END))) AS TOTALPRICE

FROM SOItem
    INNER JOIN SO ON SO.ID = SOItem.SOID
    INNER JOIN Customer ON Customer.ID = SO.customerID
WHERE SOItem.typeID = 20
GROUP BY customer.id,CUSTOMER.NAME
) as cTRet on cTret.cid=customer.id

LEFT JOIN(
      		SELECT so.customerid, SUM(amount) AS totalPaid
      		FROM posTransaction
      		INNER JOIN so ON so.id=posTransaction.soid
      		GROUP BY so.customerid
   		) postCatch ON postCatch.customerid = customer.id

WHERE (DATEDIFF(CURRENT_DATE, maxso.lastorderdate) BETWEEN $P{DormantPeriod} AND $P{LastOrderLimit(Days)}
  AND COALESCE(accountgroup.id,0) IN ($P!{customerGroupID})
  AND customer.activeFlag IN (1, $P!{ckShowInactiveCustomers})
  AND COALESCE(sysuser.username,'') LIKE $P{salesPerson}
  AND COALESCE(stateconst.name,'') LIKE $P!{cbState/Province}
  AND COALESCE(UPPER($P!{cfFilter}),'') LIKE UPPER($P{cfFilterData})
  AND customer.id LIKE $P{customerID}
  AND $P!{cbLastOrdered} = maxso.lastorderdate
  AND (ordercount.total > $P{WithTotalOrdersGreaterThan} OR COALESCE(ordercount.total, -1) = -1)
  AND (postCatch.totalpaid > $P{WithTotalSpentGreaterThan} OR COALESCE(postCatch.totalpaid, -1) = -1)
and coalesce(CustomFieldByName(customer.CUSTOMFIELDS, 'Marketing Data'),'') like concat('%',$P{MarkertingData},'%'))

GROUP BY customer.id

ORDER BY $P!{cbSortBy}]]>
		</queryString>
