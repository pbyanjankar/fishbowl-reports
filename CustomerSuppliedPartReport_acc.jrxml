		<group name="Date">
			<groupExpression><![CDATA[new SimpleDateFormat($V{DateFormat}).format($F{EVENTDATE})+$F{WONUMBER}+$F{RAWGOODLOTNUMBER}+$F{FINISHEDGOODLOTNUMBER}]]></groupExpression>
