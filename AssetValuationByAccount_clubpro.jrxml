	<parameter name="locationGroupID" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT
    COALESCE(partcost.totalcost, 0) AS AverageTotalCost,
    (COALESCE(part.stdcost, 0) * COALESCE(partcost.qty, 0)) AS StandardTotalCost,
    SUM(COALESCE(costlayer.totalcost, 0)) AS LIFOFIFOTotalCost,
    Part.num AS PartNumber,
    Part.Description As PartDescription,
    COALESCE(asaccount.name, 'Use Default') AS "InventoryAccount",
    company.name AS company,
    currency.symbol,

    concat(locationgroup.name) AS LG,
    qoh.qty AS QOH,

    (COALESCE(PartCostHistory.stdcost, 0) * COALESCE(PartCostHistory.quantity, 0)) AS HistoricalStandardTotalCost,
    (COALESCE(PartCostHistory.nextcost, 0) * COALESCE(PartCostHistory.quantity, 0)) AS HistoricalLIFOFIFOTotalCost,
    (COALESCE(PartCostHistory.avgcost, 0) * COALESCE(PartCostHistory.quantity, 0)) AS HistoricalAverageTotalCost



FROM Part
    LEFT JOIN partcost ON partcost.partid = part.id
    JOIN (SELECT max(partcosthistory.id) AS costid, partcosthistory.partid AS partcostid
           FROM partcosthistory
           WHERE partcosthistory.datecaptured <= DATE_SUB($P{PartCostAsOfDate}, INTERVAL 1 SECOND)
           GROUP BY partcosthistory.partid) AS maxcostdate ON part.id = maxcostdate.partcostid
    LEFT JOIN PartCostHistory ON maxcostdate.costid = partcosthistory.id
    LEFT JOIN costlayer ON (costlayer.partid = part.id AND costlayer.statusid = 10)
    LEFT OUTER JOIN inventorylog ON inventorylog.id = costlayer.recordId AND costlayer.tableId = -1081854656
    LEFT OUTER JOIN locationgroup ON locationgroup.id = inventorylog.locationgroupid
    LEFT JOIN asaccount ON part.inventoryaccountid = asaccount.id
    JOIN company ON company.id = 1
    LEFT JOIN currency ON currency.homeCurrency = 1
    LEFT JOIN (SELECT SUM(qtyonhand.qty) AS qty, qtyonhand.partid AS partid, qtyonhand.locationgroupid AS locationgroupid
		FROM qtyonhand
		GROUP BY partid, locationgroupid) AS qoh ON qoh.partId = part.id AND locationgroup.id = qoh.locationgroupid


WHERE part.typeid = 10
  AND UPPER(part.num) LIKE UPPER($P{partNum})
  AND UPPER(coalesce(asaccount.name, 'Use Default')) LIKE UPPER($P{AssetAccount})
  AND IF($P{HideInactive}=true,part.activeflag = 1,1=1)
  AND (locationgroup.id IN ($P!{locationGroupID}) OR locationgroup.id IS NULL)
  AND if(LOCATE(',',$P{locationGroupID})>0, 1=1,locationgroup.id IN ($P!{locationGroupID}))


GROUP BY 1,2,4,5,6,7, currency.symbol, 9 ,10

HAVING IF($P{HideZeroQty}=1,SUM(qoh.qty>0),1=1)

ORDER BY 6, part.num]]>
	</queryString>
