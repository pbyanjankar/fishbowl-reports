	<parameter name="productTree1" class="java.lang.String" isForPrompting="false"/>
	<parameter name="productTree2" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT
   part.num,
   location.name AS location,
   serialnum.serialnum
FROM product
    JOIN part ON product.partid = part.id
    JOIN tag ON tag.partid = part.id
    JOIN location ON tag.locationid = location.id
    JOIN serial ON tag.id = serial.tagid
    JOIN serialnum ON serial.id = serialnum.serialid
    JOIN parttracking ON parttracking.id = serialnum.parttrackingid AND parttracking.name = 'Serial Number'
    LEFT JOIN producttotree ON product.id = producttotree.productid
    LEFT JOIN producttree ON producttotree.producttreeid = producttree.id
WHERE
  UPPER(product.num) LIKE UPPER($P{productNum})
  AND location.id LIKE $P{locationID}
  AND ((COALESCE(producttreeid,0) IN ($P!{productTree1})) OR ((COALESCE(producttreeid,0) LIKE $P{productTree2})))
ORDER BY part.num, serialnum.serialnum]]>
	</queryString>
