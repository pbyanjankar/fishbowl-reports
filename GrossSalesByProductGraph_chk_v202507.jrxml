	<queryString>
		<![CDATA[SELECT
PRODUCT.NUM AS PRODUCTNUM,
PRODUCT.DESCRIPTION AS PRODUCTDESCRIPTION,
PRODUCT.PRICE,
uom.code AS UOM,
PRODUCT.UPC,
product.dateCreated AS PRODUCTCREATEDDATE,
COALESCE(result.salesQty,0) AS SALESQTY,
result.LASTSOLD,
COMPANY.NAME AS COMPANY,
CURRENCY.SYMBOL
FROM product
LEFT OUTER JOIN
	(SELECT product.id AS productId,
   		 SUM(if(soitem.typeid = 20,SOITEM.QTYFULFILLED * -1,SOITEM.QTYFULFILLED)) AS salesQty,
    		MAX(so.dateCompleted) AS LASTSOLD
		FROM SOItem
    	JOIN Product ON Product.ID = SOItem.productID
    	JOIN SO ON SO.ID = SOItem.SOID
    	JOIN customer ON customer.id = so.customerId
	WHERE soitem.qtyfulfilled > 0
 	 AND soitem.datelastfulfillment BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)
  	AND soitem.typeid IN (10,11,12,$P{ckIncludeReturns},21,60,80)
  	AND customer.id LIKE '$P!{customerID}'

	GROUP BY 1

	HAVING salesQty > 0) AS result ON result.productId = product.id

LEFT OUTER JOIN part ON part.id = product.partId
LEFT OUTER JOIN company ON company.id = 1
LEFT OUTER JOIN currency ON currency.homeCurrency = 1
LEFT OUTER JOIN uom ON uom.id = product.uomid

LEFT OUTER JOIN (
        SELECT
            partId,
            coalesce(SUM(qtyOnHand) -  SUM(qtyAllocated) -
            SUM(qtyNotAvailable) +
            SUM(qtyDropship),0)
            AS available
        FROM
            qtyInventoryTotals
        GROUP BY partId
    ) AS inventoryTotals ON inventoryTotals.partId = part.id

LEFT OUTER JOIN ( SELECT DISTINCT producttotree.productid AS productId, producttree.name AS category
	      FROM producttotree
    	      INNER JOIN producttree ON producttotree.producttreeid = producttree.id
	      WHERE producttree.name = "Seasons"
    ) AS SeasonalItems ON SeasonalItems.productId = product.id

LEFT JOIN producttotree ON product.id = producttotree.productid
LEFT JOIN producttree ON producttotree.producttreeid = producttree.id

WHERE
  product.num LIKE $P{productNum}
  AND ((COALESCE(producttreeid,0) IN ($P!{productTree1})) OR ((COALESCE(producttreeid,0) LIKE $P{productTree2})))
  AND if($P{ShowOutOfStockItems}, true, inventoryTotals.available>0)
  AND if($P{ShowSeasonalItems}, true, SeasonalItems.category IS NULL)
  AND CASE
	WHEN $P{ShowSalesQtyAboveZero} = true AND $P{ShowZeroQtySales} = true THEN (salesQty > -1 OR salesQty IS NULL)
	WHEN $P{ShowSalesQtyAboveZero} = true AND $P{ShowZeroQtySales} = false THEN salesQty > 0
          	WHEN $P{ShowSalesQtyAboveZero} = false AND $P{ShowZeroQtySales} THEN (salesQty = 0 OR salesQty IS NULL)
	ELSE coalesce(salesQty,0) < 0
       END


ORDER BY product.dateCreated desc, 1, 2

LIMIT $P{PartsToShow};]]>
	</queryString>
