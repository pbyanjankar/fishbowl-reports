	<queryString>
		<![CDATA[select
tag.num AS TAGNUMBER,
tag.qty AS TAGQTY,
tag.locationId AS LOCATIONID,
WarehouseReceiptNumber.info AS WRNUMBER,
ImportReferenceNumber.info AS IMPREFNUMBER,
InvoiceNumber.info AS INVOICENUMBER,

Job.info AS JOB,
Ven.info AS VENDOR,
POANumber.info AS POANUMBER,
PONumber.info AS PONUMBER,
LotNumber.info AS LOTNUMBER,


location.name AS LOCATIONNAME,
locationgroup.name AS LOCATIONGROUPNAME,
part.num AS PARTNUM,
part.num AS PARTNUMBER,
part.description AS PARTDESCRIPTION,
COALESCE(PART.STDCOST, 0) AS STDCOST,
COALESCE(PARTCOST.AVGCOST, 0) AS AVGCOST,
COALESCE((SELECT (costlayer.orgtotalcost / costlayer.orgqty) AS cost
       FROM costlayer
      WHERE costlayer.partid = part.id
        AND costlayer.statusid = 10
      ORDER BY costlayer.id ASC LIMIT 1),0) AS FIFOCOST,
    COALESCE((SELECT (costlayer.orgtotalcost / costlayer.orgqty) AS cost
       FROM costlayer
      WHERE costlayer.partid = part.id
        AND costlayer.statusid = 10
      ORDER BY costlayer.id DESC LIMIT 1),0) AS LIFOCOST,
uom.code AS UOM,
company.name AS COMPANY,
COALESCE(INVENTORYLOG.QTYONHAND, 0) AS QTYONHAND
from part
LEFT JOIN tag ON part.id = tag.partId
LEFT JOIN tagTrackingView WarehouseReceiptNumber ON WarehouseReceiptNumber.tagid = tag.id AND WarehouseReceiptNumber.partid = part.id AND WarehouseReceiptNumber.ABBR = "WR#"
LEFT JOIN tagTrackingView ImportReferenceNumber ON ImportReferenceNumber.tagid = tag.id AND ImportReferenceNumber.partid = part.id AND ImportReferenceNumber.ABBR = "ImpRef#"
LEFT JOIN tagTrackingView InvoiceNumber ON InvoiceNumber.tagid = tag.id AND InvoiceNumber.partid = part.id AND InvoiceNumber.ABBR = "Inv#"

LEFT JOIN tagTrackingView Job ON Job.tagid = tag.id AND Job.partid = part.id AND Job.ABBR = "Job"
LEFT JOIN tagTrackingView Ven ON Ven.tagid = tag.id AND Ven.partid = part.id AND Ven.ABBR = "Ven"
LEFT JOIN tagTrackingView POANumber ON POANumber.tagid = tag.id AND POANumber.partid = part.id AND POANumber.ABBR = "POA#"
LEFT JOIN tagTrackingView PONumber ON PONumber.tagid = tag.id AND PONumber.partid = part.id AND PONumber.ABBR = "PO#"
LEFT JOIN tagTrackingView LotNumber ON LotNumber.tagid = tag.id AND LotNumber.partid = part.id AND LotNumber.ABBR = "Lot#"

LEFT JOIN location ON location.id = tag.locationId
LEFT JOIN locationgroup ON locationgroup.id = location.locationgroupid
LEFT JOIN uom ON part.uomid = uom.id
LEFT JOIN partcost ON part.id = partcost.partid
JOIN company ON company.id = 1
INNER JOIN (SELECT max(inventorylog.id) AS logid, inventorylog.partid AS logpartid,
              inventorylog.locationgroupid AS loglocationgroupid
            FROM inventorylog
           WHERE inventorylog.eventdate <= DATE_SUB($P{ReportOnDate}, INTERVAL 1 SECOND)
             AND inventorylog.typeid IN (1,10,15,20,30,40,50,60,64,65,67,68,72)
           GROUP BY inventorylog.partid, inventorylog.locationgroupid
           ) AS maxlogdate ON part.id = maxlogdate.logpartid AND maxlogdate.loglocationgroupid = locationgroup.id
    LEFT JOIN inventorylog ON maxlogdate.logid = inventorylog.id
WHERE part.typeid = 10
  AND locationgroup.id IN ($P!{locationGroupID})
  AND inventorylog.typeid IN (1,10,15,20,30,40,50,60,64,65,67,68,72)
  AND part.activeflag IN ($P!{ckShowInactiveParts},1)
  AND part.num LIKE $P{partNum}
  AND if($P{cbJobFilter} = "", true, UPPER(Job.info) = UPPER($P{cbJobFilter}))
  AND if($P{BuyerPO#}="%", true, UPPER(PONumber.info) =  UPPER($P{BuyerPO#}))
  AND if($P{poNum}="%", true, UPPER(POANumber.info) =  UPPER($P{poNum}))
  AND if($P{vendorID}="%", true, UPPER(Ven.info) = (SELECT name FROM vendor WHERE COALESCE(vendor.id, 0) LIKE $P{vendorID}))

  ORDER BY locationgroup.name, part.num]]>
	</queryString>
