	<parameter name="ReportTitle" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT
company.name AS company,
part.id as partid,
part.num AS PARTNUMBER,
part.description AS PARTDESCRIPTION,
uom.code AS UOM,
COALESCE(endInventorylog.endQty,0) AS ENDQTY,
COALESCE(startInventorylog.startQty,0) AS STARTQTY,
COALESCE(INVENTORYTOTALS.TOTALONHAND,0) AS ONHAND,
COALESCE(INVENTORYTOTALS.TOTALNOTAVAILABLE,0) AS UNAVAILABLE,
COALESCE(INVENTORYTOTALS.TOTALDROPSHIP,0) AS DROPSHIP,
COALESCE(COMMITTED.TOTAL,0) AS QTYCOMMITTED,
COALESCE(INVENTORYTOTALS.TOTALALLOCATED,0) AS ALLOCATED,
COALESCE(INVENTORYTOTALS.TOTALONORDER,0) AS ONORDER,
COALESCE(INVENTORYTOTALS.totalNotAvailabletopick,0) totalNotAvailabletopick
FROM part
LEFT OUTER JOIN uom ON uom.id = part.uomid
JOIN company ON company.id = 1
LEFT JOIN currency ON currency.homeCurrency = 1
LEFT OUTER JOIN (SELECT
    	partId,
          SUM(qtyOnHand) AS totalOnHand,
          SUM(qtyNotAvailable) AS totalNotAvailable,
          SUM(qtyNotAvailabletopick) AS totalNotAvailabletopick,
          SUM(qtyDropship) AS totalDropship,
          SUM(qtyAllocated) AS totalAllocated,
          SUM(qtyOnOrder) AS totalOnOrder
    FROM
          qtyInventoryTotals
    GROUP BY partId
) AS inventoryTotals ON inventoryTotals.partId = part.id
LEFT OUTER JOIN (
  	SELECT
            partId,
            SUM(qty) AS total
        FROM
            qtyCommitted
        GROUP BY partId
    ) AS committed ON committed.partId = part.id

INNER JOIN (
	SELECT
	   partid,
	   sum(inventorylog.qtyonhand) AS endQty
	FROM inventorylog
	INNER JOIN (
		SELECT max(inventorylog.id) AS logid,
			inventorylog.partid AS logpartid,
              		inventorylog.locationgroupid AS loglocationgroupid
            	FROM inventorylog
           	WHERE inventorylog.eventdate <= DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)
             	AND inventorylog.typeid IN (1,10,15,20,30,40,50,60,64,65,67,68,72,90)
          		GROUP BY inventorylog.partid, inventorylog.locationgroupid) AS maxlogdate ON maxlogdate.logid = inventorylog.id
	GROUP BY partId ) AS endInventoryLog ON endInventoryLog.partId = part.id

LEFT JOIN (
	SELECT
	   partid,
	   sum(inventorylog.qtyonhand) AS startQty
	FROM inventorylog
	INNER JOIN (
          		SELECT
              		max(inventorylog.id) AS logid,
              		inventorylog.partid AS logpartid,
              		inventorylog.locationgroupid AS loglocationgroupid
          		FROM inventorylog
           	WHERE inventorylog.eventdate <= $P{dateRange1}
             	AND inventorylog.typeid IN (1,10,15,20,30,40,50,60,64,65,67,68,72,90)
           	GROUP BY inventorylog.partid, inventorylog.locationgroupid) AS startlogdate ON startlogdate.logid = inventorylog.id
	GROUP BY partId ) AS startInventoryLog ON startInventoryLog.partId = part.id

WHERE part.num LIKE $P{partNum}

order by part.num]]>
	</queryString>
