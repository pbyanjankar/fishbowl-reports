	<subDataset name="RawGoods(Detail1)" uuid="278b12ce-bfd4-4519-ab6e-ca45f91361b5">
		<parameter name="bomNum" class="java.lang.String"/>
		<queryString>
			<![CDATA[select
    part.description,
    vendor.name as vendor,
    vendorparts.vendorpartnumber,
    part.num as partnum,
    -- round(bomitem.quantity * 454 / coalesce(casepack.info, 1), 1) as targetweight,
    round(bomitem.quantity * 454 / 1, 1) as targetweight,
    customfieldbyid(part.Customfields, 76) as 'allergen',
    -- bomitem.instructionnote as plusminus,
    'Bomitem Instruction Missing' as plusminus,
    customfieldbyid(part.Customfields, 79) as 'total weight',
    customfieldbyid(part.Customfields, 78) as 'declared weight',
    customfieldbyid(product.customfields, 48) as 'casepack'
from bom
inner join bomitem on bomitem.bomid = bom.id and bomitem.typeid = 20
inner join part on bomitem.partid = part.id
    and (part.description LIKE '%Blend%'
         or part.description LIKE '%Raw Ingredient%'
         or part.description LIKE '%Seasoning%')
inner join bomitem fg on fg.bomid = bom.id and fg.typeid = 10
inner join part fgpart on fgpart.id = fg.partid
left join product on product.partid = fgpart.id
left join vendorparts on vendorparts.partid = part.id and vendorparts.defaultflag = 1
left join vendor on vendorparts.vendorid = vendor.id
where bom.num = $P{bomNum}]]>
		</queryString>
		<field name="DESCRIPTION" class="java.lang.String"/>
		<field name="VENDOR" class="java.lang.String"/>
		<field name="VENDORPARTNUMBER" class="java.lang.String"/>
		<field name="PARTNUM" class="java.lang.String"/>
		<field name="TARGETWEIGHT" class="java.lang.Double"/>
		<field name="ALLERGENS" class="java.lang.String"/>
		<field name="PLUSMINUS" class="java.lang.String"/>
		<field name="TOTALWEIGHT" class="java.lang.String"/>
		<field name="DECLAREDWEIGHT" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Coding Spec (Detail2)" uuid="d1428174-fc0e-447f-b872-807c43050946">
		<parameter name="bomNum" class="java.lang.String"/>
		<queryString>
			<![CDATA[select
    part.description,
    part.num as partnum,
    fgpart.num as fgartnum,
    fgpart.description as fgpartdesc,
    case when part.description LIKE('%Lot%Code%Format%') or part.num LIKE ('%Lot%Code%Format%') then 1 else 2 end as sortflag
from bom
inner join bomitem on bomitem.bomid = bom.id and bomitem.typeid = 20
inner join part on bomitem.partid = part.id
    and (part.description LIKE 'Ink Jet%'
         or part.num LIKE 'Ink Jet%'
         or part.description LIKE('%Lot%Code%Format%')
         or part.num LIKE ('%Lot%Code%Format%'))
inner join bomitem fg on fg.bomid = bom.id and fg.typeid = 10
inner join part fgpart on fgpart.id = fg.partid
where bom.num = $P{bomNum}



order by sortflag]]>
		</queryString>
		<field name="DESCRIPTION" class="java.lang.String"/>
		<field name="PARTNUM" class="java.lang.String"/>
		<field name="FGARTNUM" class="java.lang.String"/>
		<field name="FGPARTDESC" class="java.lang.String"/>
		<field name="SORTFLAG" class="java.lang.Integer"/>
	</subDataset>
	<subDataset name="Case Packaging SPec (Detail3)" uuid="a8bbe432-8155-44af-9c0c-bcd22b3f4298">
		<parameter name="bomNum" class="java.lang.String"/>
		<queryString>
			<![CDATA[select
    -- customfieldbyid(part.Customfields, 77) as 'case packing instructions'
    customfieldbyid(fgpart.Customfields, 77) as 'instructions'
from bom
inner join bomitem fg on fg.bomid = bom.id and fg.typeid = 10
inner join part fgpart on fgpart.id = fg.partid
where bom.num = $P{bomNum}]]>
		</queryString>
		<field name="INSTRUCTIONS" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Packaging Materials (Detail4)" uuid="13cf0ef6-c068-424b-83be-aa90de240552">
		<parameter name="bomNum" class="java.lang.String"/>
		<parameter name="module" class="java.lang.Object" isForPrompting="false">
			<defaultValueExpression><![CDATA[null]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
    part.num as partnum,
    part.description,
    customfieldbyid(BOM.Customfields, 86) as 'Vince',
    customfieldbyid(BOM.Customfields, 87) as 'Mitch',
    part.id as PACKAGINGSHIPPINGID,
    vendor.name as vendor,
    vendorparts.vendorpartnumber,
    part.revision as revision
from bom
inner join bomitem on bomitem.bomid = bom.id and bomitem.typeid = 20
inner join part on bomitem.partid = part.id
    and (part.description LIKE '%Carton%'
         or part.description LIKE '%Label%'
         or part.description LIKE '%Packaging%'
         or part.description LIKE '%Shipper%'
         or part.description LIKE '%Web%')
    and part.typeid = 10
inner join bomitem fg on fg.bomid = bom.id and fg.typeid = 10
left join vendorparts on vendorparts.partid = part.id and vendorparts.defaultflag = 1
left join vendor on vendorparts.vendorid = vendor.id
where bom.num = $P{bomNum}]]>
		</queryString>
		<field name="PARTNUM" class="java.lang.String"/>
		<field name="DESCRIPTION" class="java.lang.String"/>
		<field name="MITCH" class="java.lang.Integer"/>
		<field name="VINCE" class="java.lang.Integer"/>
		<field name="PACKAGINGSHIPPINGID" class="java.lang.Integer"/>
		<field name="VENDOR" class="java.lang.String"/>
		<field name="VENDORPARTNUMBER" class="java.lang.String"/>
		<field name="REVISION" class="java.lang.String"/>
	</subDataset>
	<parameter name="bomNum" class="java.lang.String"/>
	<parameter name="module" class="java.lang.Object" isForPrompting="false">
		<defaultValueExpression><![CDATA[null]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
    fgpart.num as fgpart,
    fgpart.datecreated,
    fgpart.revision as revision,
    fgpart.description as fgpartdesc,
    fgpart.upc,
    round(coalesce(fgpart.weight, 1), 1) as fgpartweight,
    round(coalesce(fgpart.len, 1), 3) as len,
    round(coalesce(fgpart.height, 1), 3) as height,
    round(coalesce(fgpart.width, 1), 3) as width,
    -- customfieldbyid(part.customfields, 26) as Block,
    -- customfieldbyid(part.customfields, 27) as Tiers,
    -- customfieldbyid(part.customfields, 28) as casespallet,
    -- customfieldbyid(part.customfields, 81) as casecode,
    customfieldbyid(fgpart.customfields, 26) as Block,
    customfieldbyid(fgpart.customfields, 27) as Tiers,
    customfieldbyid(fgpart.customfields, 28) as casespallet,
    customfieldbyid(fgpart.customfields, 81) as casecode,
    bom.note as bomnote,
    1 as PALLETPATTERNID1,
    2 as PALLETPATTERNID2,
    -- customfieldbyid(BOM.Customfields, 86) as 'Approved by Vince',
    -- customfieldbyid(BOM.Customfields, 87) as 'Approved by Mitch',
    customfieldbyid(BOM.Customfields, 86) as 'Vince',
    customfieldbyid(BOM.Customfields, 87) as 'Mitch',
    stackingshipping.instructions as StackingAndShippingInsytuctions,
    tops.topid as TOPID
from bom
inner join bomitem fg on fg.bomid = bom.id and fg.typeid = 10
inner join part fgpart on fgpart.id = fg.partid
left join (select part.description as instructions
           from bom
           inner join bomitem on bomitem.bomid = bom.id and bomitem.typeid = 20
           and bom.num = $P{bomNum}
           inner join part on part.id = bomitem.partid and part.num LIKE ('Stacking%Shipping%Instructions%')) stackingshipping
    on 1 = 1
left join (select part.id as topid
           from bom
           inner join bomitem on bomitem.bomid = bom.id and bomitem.typeid = 20
           and bom.num = $P{bomNum}
           inner join part on part.id = bomitem.partid and part.num LIKE ('TOPS%')
           group by topid) tops on 1 = 1
where bom.num = $P{bomNum}]]>
	</queryString>
