	<subDataset name="ItemAdjustHistory" uuid="4c2382f0-8806-4b18-ac70-9e5e8d493117">
		<parameter name="partId" class="java.lang.String" isForPrompting="false">
			<defaultValueExpression><![CDATA["-1"]]></defaultValueExpression>
		</parameter>
		<parameter name="dateRange1" class="java.util.Date" isForPrompting="false">
			<parameterDescription><![CDATA[This Month]]></parameterDescription>
			<defaultValueExpression><![CDATA[new Date()]]></defaultValueExpression>
		</parameter>
		<parameter name="dateRange2" class="java.util.Date" isForPrompting="false">
			<defaultValueExpression><![CDATA[new Date()]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
inventorylog.eventdate,
inventorylog.changeqty as qty,
    CASE WHEN inventorylog.typeid in (30,40)
              THEN 'ZZ'
              ELSE CASE WHEN inventorylog.typeid in (50)
                        THEN 'CC'
		   WHEN inventorylog.typeid = 10 AND inventorylog.tableid = -355941248 THEN 'BB'
                        ELSE 'AA'
                   END
         END as adjCategory,
   wo.num AS WONUMBER,
   trackinginfo.info AS RAWGOODLOTNUMBER,
   finishedGood.lotnumber AS FINISHEDGOODLOTNUMBER
from inventorylog
	JOIN location beglocation ON inventorylog.beglocationid = beglocation.id
	JOIN locationgroup beglocationgroup ON beglocation.locationgroupid = beglocationgroup.id
	JOIN location endlocation ON inventorylog.endlocationid = endlocation.id
	JOIN locationgroup endlocationgroup ON endlocation.locationgroupid = endlocationgroup.id
LEFT OUTER JOIN woitem ON woitem.id =  inventorylog.recordid AND inventorylog.tableid = -355941248
LEFT OUTER JOIN wo ON woitem.woid = wo.id
LEFT OUTER JOIN trackinginfo ON trackinginfo.tableid = -355941248 and woitem.id = trackinginfo.recordid and trackinginfo.parttrackingid=1
LEFT OUTER JOIN (
			SELECT woitem.woid, trackinginfo.info as lotnumber FROM woitem
			INNER JOIN trackinginfo ON trackinginfo.tableid = -355941248 and woitem.id = trackinginfo.recordid and trackinginfo.parttrackingid=1
			WHERE woitem.typeid IN (10,31)
) AS finishedGood ON finishedGood.woid = wo.id


where inventorylog.typeid in (CASE WHEN inventorylog.typeid = 10 AND inventorylog.tableid IS NULL THEN 10 ELSE 0 END,
		         CASE WHEN inventorylog.typeid = 10 AND inventorylog.tableid = -1081854656 THEN 10 ELSE 0 END,
		         CASE WHEN inventorylog.typeid = 10 AND inventorylog.tableid = -355941248 THEN 10 ELSE 0 END,30,40,50,60,64,65,67,90)
and inventorylog.partid = $P{partId}
and inventorylog.eventdate between $P{dateRange1} and $P{dateRange2}
and CASE
	WHEN inventorylog.typeid IN (30,40) AND beglocationgroup.id = endlocationgroup.id THEN 0
	ELSE inventorylog.changeqty
    END <> 0

order by adjcategory, inventorylog.eventdate]]>
		</queryString>
		<field name="QTY" class="java.lang.Double"/>
		<field name="ADJCATEGORY" class="java.lang.String"/>
		<field name="EVENTDATE" class="java.sql.Timestamp"/>
		<field name="WONUMBER" class="java.lang.String"/>
		<field name="RAWGOODLOTNUMBER" class="java.lang.String"/>
		<field name="FINISHEDGOODLOTNUMBER" class="java.lang.String"/>
		<variable name="TotalQty" class="java.lang.Double" calculation="Sum">
