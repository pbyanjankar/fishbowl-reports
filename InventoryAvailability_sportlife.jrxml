		<queryString>
			<![CDATA[SELECT
    PART.NUM AS PART,
    PART.DESCRIPTION,
    UOM.CODE AS UOMCODE,
    (SELECT NAME FROM COMPANY WHERE ID = 1) AS COMPANY,
    COALESCE(INVENTORYTOTALS.TOTALONHAND,0) AS QTY,
    COALESCE(INVENTORYTOTALS.TOTALNOTAVAILABLE,0) AS UNAVAILABLE,
    COALESCE(INVENTORYTOTALS.TOTALDROPSHIP,0) AS DROPSHIP,
    COALESCE(COMMITTED.TOTAL,0) AS QTYCOMMITTED,
    COALESCE(INVENTORYTOTALS.TOTALALLOCATED,0) AS ALLOCATED,
    COALESCE(INVENTORYTOTALS.TOTALONORDER,0) AS ONORDER,
    COALESCE(INVENTORYTOTALS.totalNotAvailabletopick,0) totalNotAvailabletopick,
    $P!{CustomField:Part1} AS partcf1,
    $P!{CustomField:Part2} AS partcf2,
    $P!{CustomField:Part3} AS partcf3

FROM
    part
    LEFT JOIN uom ON part.uomid = uom.id
    LEFT JOIN (
        SELECT
            partId,
            SUM(qtyOnHand) AS totalOnHand,
            SUM(qtyNotAvailable) AS totalNotAvailable,
            SUM(qtyNotAvailabletopick) AS totalNotAvailabletopick,
            SUM(qtyDropship) AS totalDropship,
            SUM(qtyAllocated) AS totalAllocated,
            SUM(qtyOnOrder) AS totalOnOrder
        FROM
            qtyInventoryTotals
        WHERE locationGroupId IN ($P!{locationGroupID})
	  OR locationGroupId IN ($P!{locationGroupIDLocationGroup2})
	  OR locationGroupId IN ($P!{locationGroupIDLocationGroup3})
	  OR locationGroupId IN ($P!{locationGroupIDLocationGroup4})
	  OR locationGroupId IN ($P!{locationGroupIDLocationGroup5})
	  OR locationGroupId IN ($P!{locationGroupIDLocationGroup6})
        GROUP BY partId
    ) AS inventoryTotals ON inventoryTotals.partId = part.id
    LEFT JOIN (
        SELECT
            partId,
            SUM(qty) AS total
        FROM
            qtyCommitted
        WHERE locationGroupId IN ($P!{locationGroupID})
	  OR locationGroupId IN ($P!{locationGroupIDLocationGroup2})
	  OR locationGroupId IN ($P!{locationGroupIDLocationGroup3})
	  OR locationGroupId IN ($P!{locationGroupIDLocationGroup4})
	  OR locationGroupId IN ($P!{locationGroupIDLocationGroup5})
	  OR locationGroupId IN ($P!{locationGroupIDLocationGroup6})
        GROUP BY partId
    ) AS committed ON committed.partId = part.id

WHERE
    part.num LIKE $P{partNum}
    AND part.id != 0
    AND part.typeid = 10
    AND part.activeflag = 1
    AND COALESCE(UPPER($P!{cfFilter}),'') LIKE UPPER($P{cfFilterData})
HAVING IF($P{ShowPartsWithZeroQuantity}, 1=1, (qty + unavailable + dropship + qtycommitted + allocated + onorder) > 0)

ORDER BY part.num

$P!{Count}
$P!{Skip}]]>
		</queryString>
