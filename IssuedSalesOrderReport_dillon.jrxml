		<parameter name="cbApplyDateFilter" class="java.lang.String" isForPrompting="false">
			<property name="Group" value="Select a date field and enter a date range"/>
			<property name="Sort" value="2"/>
			<parameterDescription><![CDATA[so.dateFirstShip,Date Scheduled,so.dateIssued,Date Issued]]></parameterDescription>
			<defaultValueExpression><![CDATA["so.datefirstship"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
    so.dateFirstShip AS DATEFIRSTSHIP,
    CUSTOMER.NAME AS CUSTOMER,
    so.num AS SONUMBER,
    sysuser.firstname AS SALESPERSONFIRSTNAME,
    sysuser.lastname AS  SALESPERSONLASTNAME,
    paymentterms.name AS PAYMENTTERMS,
    carrier.name AS CARRIERNAME,
    so.customerpo AS CUSTOMERPO,
    CURRENCY.SYMBOL,
    so.totalprice AS FPRICE

FROM
    so
    INNER JOIN customer ON customer.id = so.customerid
    LEFT OUTER JOIN currency ON currency.homeCurrency = 1
    LEFT OUTER JOIN sysuser ON sysuser.id = so.salesmanId
    LEFT OUTER JOIN carrier ON so.carrierid = carrier.id
    LEFT OUTER JOIN paymentterms ON so.paymenttermsid = paymentterms.id

WHERE
    so.customerid LIKE $P{customerID}
    AND UPPER(so.salesman) LIKE UPPER($P{salesPerson})
    AND so.statusid IN (20)
    AND $P!{cbApplyDateFilter} BETWEEN $P{dateRange1} AND DATE_SUB(DATE_ADD($P{dateRange2},INTERVAL 1 DAY), INTERVAL 1 SECOND)

ORDER BY
    so.dateFirstShip;]]>
		</queryString>
