	<queryString>
		<![CDATA[select
company.name as company,
customer.name as Customer,
so.customerpo,
so.num as sonum,
sostatus.name as Status,
so.datecreated as DateCreated,
coalesce(so.datecompleted, so.datefirstship) as DateFulfilledOrScheduled,
so.shiptoname as StoreNum,
so.shiptoaddress AS SHIPTOADDRESS,
so.shiptocity AS SHIPTOCITY,
sstate.code AS SHIPTOSTATE,
so.shiptozip AS SHIPTOZIP,
soitem.productnum as Product,
soitem.description,
soitem.qtytofulfill as Qty,
so.note as Notes,
coalesce(memos.memos,'') as memos

from so
inner join company on company.id = 1
inner join sostatus on sostatus.id = so.statusid
inner join soitem on soitem.soid = so.id and soitem.typeid NOT IN (40,50,70)
inner join customer on so.customerid = customer.id
left join stateconst sstate on sstate.id = so.shiptostateid
left join (select group_concat(distinct memo.memo) as memos, memo.recordid
             from memo
               where tableid = 1012013120
                 group by recordid) memos on memos.recordid = so.id

where so.datecreated between $P{dateRange1} and $P{dateRange2}
  and so.num LIKE ($P{soNum})
  and upper(so.customerpo) LIKE concat('%',upper($P{customerPO}),'%')
  and so.customerid LIKE ($P{customerID})
  and soitem.productnum LIKE ($P{productNum})
  and so.statusid IN ($P{ckEstimate},$P{ckIssued},$P{ckInProgress},$P{ckFulfilled},$P{ckClosedShort},$P{ckVoided})
  and soitem.statusid IN ($P{ckPicking},$P{ckPicked},$P{ckPartial},$P{ckFulfilledSOItem})
  and  soitem.typeid IN ($P{ckSale},$P{ckCreditReturn},$P{ckDropShip})

order by customer, sonum, datecreated]]>
	</queryString>
