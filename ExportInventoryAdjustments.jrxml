    INNER JOIN company ON company.id = 1
	left join inventorylog on (post.id = inventorylog.recordid and post.refid = inventorylog.PARTID and inventorylog.tableid = -1407594912)

-- left join asaccount INVENTORYACCOUNT on INVENTORYACCOUNT.id = part.INVENTORYACCOUNTID
 left join asaccount ADJUSTMENTACCOUNT on ADJUSTMENTACCOUNT.id = part.ADJUSTMENTACCOUNTID
 left join asaccount ScrapAccount on ScrapAccount.id = part.SCRAPACCOUNTID


left join (select asaccount.name from asaccount inner join sysproperties on (sysproperties.syskey = 'Acct ID Inventory Adjustment' and sysproperties.sysvalue = asaccount.id)) defaultInventoryAdjustment on 1 = 1
left join (select asaccount.name from asaccount inner join sysproperties on (sysproperties.syskey = 'Acct ID Scrapped Inventory' and sysproperties.sysvalue = asaccount.id)) defaultScrappedInventory on 1 = 1


