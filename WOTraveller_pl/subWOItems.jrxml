	<parameter name="cbSortBOMItemsBy" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT part.description AS partdescription, moitem.qtytofulfill, uom.code AS uomcode,
    bomitemtype.name AS bomtype, part.num AS partnum, moitem.priceadjustment,
    bomitem.description AS BOMITEMDESCRIPTION,
    ((moitem.qtytofulfill * COALESCE(uomconversion.multiply,1)) / COALESCE(uomconversion.factor,1)) AS qtyordered,
    homeCurrency.symbol AS HOMECURRENCYSYMBOL

FROM moitem
    INNER JOIN part ON moitem.partid = part.id
    INNER JOIN soitem ON soitem.id = moitem.soitemid
    INNER JOIN product ON product.id = soitem.productId
    INNER JOIN bomitemtype ON moitem.typeid = bomitemtype.id
    LEFT OUTER JOIN bomitem ON bomitem.id = moitem.bomitemid
    INNER JOIN uom ON part.uomid = uom.id
    LEFT OUTER JOIN uomconversion ON touomid = part.uomid AND fromuomid = moitem.uomid
    LEFT OUTER JOIN currency AS homeCurrency ON homeCurrency.homeCurrency = 1

WHERE moitem.soitemid = $P{soItemID}
  AND moitem.typeid IN (20,30)
  AND bomitem.bomItemGroupId IS NOT NULL

ORDER BY
    CASE
        WHEN  $P{cbSortBOMItemsBy}='BOMItemDescptionASC' THEN bomitem.description
    END ASC,
    CASE
	WHEN  $P{cbSortBOMItemsBy}='BOMConfig' THEN
         		 moitem.typeid
    END ASC,
    CASE
	WHEN  $P{cbSortBOMItemsBy}='BOMConfig' THEN
		moitem.sortidinstruct
    END ASC;]]>
	</queryString>
