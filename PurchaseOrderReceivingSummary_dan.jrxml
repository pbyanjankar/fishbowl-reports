		<queryString>
			<![CDATA[SELECT
   POITEM.DATESCHEDULEDFULFILLMENT AS POITEMDATESCHEDULEDFULFILLMENT,
   PO.NUM AS PONUM,
   COALESCE(PART.DESCRIPTION, POITEM.DESCRIPTION) AS PARTDESCRIPTION,
   COALESCE(PART.NUM, POITEM.PARTNUM) AS PARTNUM,
   UOM.CODE,
   receiptitemstatus.NAME AS ITEMSTATUS,
   CURRENCY.SYMBOL,
   receiptitem.qty AS TOTALQTY
FROM po
   LEFT JOIN poitem ON poitem.poid = po.id
   INNER JOIN part ON poitem.partid = part.id
   INNER JOIN uom ON poitem.uomid = uom.id
   INNER JOIN receiptitem ON receiptitem.poitemid = poitem.id
   INNER JOIN receiptitemstatus ON receiptitem.statusid = receiptitemstatus.id
   LEFT OUTER JOIN currency ON currency.homeCurrency = 1
WHERE
   COALESCE(part.num, '') LIKE $P{partNum}
   AND $P!{cbApplyDateFilter} BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)
    AND PO.statusID IN ($P!{ckBidRequest}, $P!{ckPendingApproval},$P!{ckIssued}, $P!{ckPicking}, $P!{ckPartial}, $P!{ckPicked}, $P!{ckFulfilled}, $P!{ckClosedShort}, $P!{ckVoided})
ORDER BY $P!{cbSortBy}]]>
		</queryString>
