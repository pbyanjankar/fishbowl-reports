		<queryString>
			<![CDATA[SELECT
    coalesce($P!{cbGroupBy}, 'None') AS GROUPBY,
    sum(postSoItem.totalPrice) AS TOTALPRICe,
    sum(postSoItem.qty) AS QTYPOSTED
FROM
    so
    INNER JOIN customer ON customer.id = so.customerid
    INNER JOIN soitem ON soitem.soid = so.id
    INNER JOIN postSoItem ON postSoItem.soItemId = soItem.id
    LEFT JOIN locationgroup ON locationgroup.id = so.locationgroupid
    LEFT JOIN stateconst ON so.shiptostateid = stateconst.id
    LEFT JOIN product ON soitem.productid = product.id
    LEFT JOIN part ON part.id = product.partId
    LEFT JOIN vendorparts ON vendorparts.partId = part.id AND vendorparts.defaultFlag = 1
    LEFT JOIN vendor ON vendor.id = vendorparts.vendorid
    LEFT JOIN currency ON currency.homeCurrency = 1
WHERE
    soitem.typeid IN (10,11,12,$P!{ckIncludeReturns},21,30,31,50,60,80)
    AND (date_format(so.dateCompleted, "%Y-%m-%d %H:%i:%s") BETWEEN $P{dateRange1SODateCompleted} AND concat(Date($P{dateRange2SODateCompleted}), " 23:59:59"))
    AND if($P{ProductDescription}='%', 1=1, UPPER(product.description) LIKE concat("%", UPPER($P{ProductDescription}), "%"))
    AND so.locationgroupid IN ($P!{locationGroupID})
    AND if($P{vendorID}='%', 1=1, vendor.id LIKE '$P!{vendorID}')
    AND UPPER(customer.name) LIKE concat("%", UPPER($P{Customer}), "%")
    AND UPPER(so.salesman) LIKE UPPER($P{salesPerson})

GROUP BY GROUPBY
ORDER BY
    GROUPBY]]>
		</queryString>
