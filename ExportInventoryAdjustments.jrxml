 AND post.refitemid IN ($P!{ckAddInitialInv},$P!{ckScrap},$P!{ckCycleCount},$P!{ckCostChange})
and post.dateposted is not null
union all
SELECT  post.id as postid, 2 as Sort, part.num as Part,  post.quantity AS Qty,uom.code AS Uomcode, ABS(post.amount) AS amount, post.datecreated AS datecreated, post.dateposted AS dateposted,
    post.refitemid AS type, inventorylog.info as Notes,
   -- coalesce(INVENTORYACCOUNT.name,coalesce( (case part.typeid when 10 then defaultInventoryAccount.name when 50 then defaultCapitalEquipment.name  else 'Improper Part Type' end),'Default Inventory Account') ) as Account,
 (case post.refitemid when 2 then coalesce(ScrapAccount.name ,defaultScrappedInventory.name) else coalesce (ADJUSTMENTACCOUNT.name, defaultInventoryAdjustment.name) end) as "Account",

