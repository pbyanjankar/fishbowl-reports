	<subDataset name="POHistory" uuid="1025e496-4960-427b-925b-47c6fcb25f2b">
		<parameter name="partId" class="java.lang.String" isForPrompting="false">
			<defaultValueExpression><![CDATA["-1"]]></defaultValueExpression>
		</parameter>
		<parameter name="dateRange1" class="java.util.Date" isForPrompting="false">
			<parameterDescription><![CDATA[This Month]]></parameterDescription>
			<defaultValueExpression><![CDATA[new Date()]]></defaultValueExpression>
		</parameter>
		<parameter name="dateRange2" class="java.util.Date" isForPrompting="false">
			<defaultValueExpression><![CDATA[new Date()]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[Select
po.num AS poNum,
postpo.postdate,
    CASE WHEN poitem.typeid = 20
        THEN -1 *((postpoitem.qty * COALESCE(uomconversion.multiply,1)) / COALESCE(uomconversion.factor,1))
        ELSE ((postpoitem.qty * COALESCE(uomconversion.multiply,1)) / COALESCE(uomconversion.factor,1))
    END AS qty,
poitemstatus.name AS ITEMSTATUS

from po
JOIN poitem ON po.id = poitem.poid
LEFT JOIN poitemstatus ON poitem.statusid = poitemstatus.id
JOIN postpoitem ON poitem.id = postpoitem.poitemid
JOIN postpo ON postpoitem.postpoid = postpo.id
JOIN part on poitem.partid = part.id
LEFT JOIN uomconversion ON (uomconversion.touomid = part.uomid AND uomconversion.fromuomid = poitem.uomid)
where part.id = $P{partId}
and postpo.postdate between $P{dateRange1} and DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)
AND poitem.typeid <> 30]]>
		</queryString>
