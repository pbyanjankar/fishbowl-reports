	<queryString>
		<![CDATA[SELECT
    COMPANY.NAME AS COMPANY,
    locationgroup.name AS LG,
    (SELECT vendor.name FROM vendor WHERE vendor.id = $P{vendorID} limit 1) AS VENDORNAME
FROM
    company
LEFT OUTER JOIN locationgroup ON locationgroup.id IN ($P!{locationGroupID})
WHERE company.id = 1
LIMIT 1]]>
