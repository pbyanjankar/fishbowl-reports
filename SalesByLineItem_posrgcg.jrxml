	<queryString>
		<![CDATA[SELECT

COMPANY.NAME AS COMPANY,
CURRENCY.SYMBOL,
PRODUCT.NUM AS PRODUCTNUM,
PRODUCT.DESCRIPTION AS PRODUCTDESCRIPTION,
so.num AS SONUMBER,
sysuser.username AS SALESPERSON,
soitemstatus.name AS STATUS,
customer.name AS CUSTOMER,

(SOITEM.UNITPRICE * SOITEM.QTYTOFULFILL) + SOITEM.ADJUSTAMOUNT AS SALES,
if(soitem.typeid = 20,SOITEM.QTYTOFULFILL * -1,SOITEM.QTYTOFULFILL) AS salesQty

FROM SOItem
    INNER JOIN soitemstatus ON soitemstatus.id = soitem.statusid
    JOIN Product ON Product.ID = SOItem.productID
    JOIN SO ON SO.ID = SOItem.SOID
    INNER JOIN customer ON customer.id = so.customerId
    LEFT OUTER JOIN sysuser ON sysuser.id = so.salesmanId
    JOIN company ON company.id = 1
    LEFT JOIN currency ON currency.homeCurrency = 1
WHERE
  UPPER(so.num) LIKE UPPER($P{soNum})
  AND so.dateIssued BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)
  AND soitem.typeid IN (10,11,12,$P{ckIncludeReturns},21,60,80)
  AND COALESCE(product.num,'') LIKE $P{productNum}

ORDER BY 3]]>
	</queryString>
