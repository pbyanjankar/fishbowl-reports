    post.refitemid AS type, inventorylog.info as Notes,
    coalesce(INVENTORYACCOUNT.name,coalesce(
(case part.typeid when 10 then defaultInventoryAccount.name when 50 then defaultCapitalEquipment.name
 else 'Improper Part Type' end),'Default Inventory Account') ) as Account,
-- (case post.refitemid when 2 then coalesce(ScrapAccount.name ,defaultScrappedInventory.name) else coalesce (INVENTORYACCOUNT.name, defaultInventoryAdjustment.name) end) as "Account",

 (case  post.refitemid when 2 then 0.00 else (case when  post.amount > 0 then   post.amount else 0.00 end ) end) as Debit,
  (case  post.refitemid when 2 then post.amount else (case when  post.amount < 0 then   post.amount * (-1) else 0.00 end ) end) as Credit



FROM post
    INNER JOIN part ON post.refid = part.id
    INNER JOIN uom ON uom.id = part.uomid
    INNER JOIN company ON company.id = 1
	left join inventorylog on (post.id = inventorylog.recordid and post.refid = inventorylog.PARTID and inventorylog.tableid = -1407594912)

 left join asaccount INVENTORYACCOUNT on INVENTORYACCOUNT.id = part.INVENTORYACCOUNTID

left join (select asaccount.name from asaccount inner join sysproperties on (sysproperties.syskey = 'Acct ID inventoryAsset' and sysproperties.sysvalue = asaccount.id)) defaultInventoryAccount on 1 = 1
left join (select asaccount.name from asaccount inner join sysproperties on (sysproperties.syskey = 'Acct ID Capital Equipment' and sysproperties.sysvalue = asaccount.id)) defaultCapitalEquipment on 1 = 1


