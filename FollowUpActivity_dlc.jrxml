		<queryString>
			<![CDATA[WITH

so_audit_efu AS (
    SELECT
        so_aud.id,
        so_aud.rev,
        so_aud.username,
        CustomFieldByName(so_aud.customfields, 'Estimate Follow Up') AS efu,
        LAG(CustomFieldByName(so_aud.customfields, 'Estimate Follow Up'))
            OVER (PARTITION BY so_aud.id ORDER BY so_aud.rev) AS prev_efu
    FROM so_aud
    INNER JOIN so ON so.id = so_aud.id
    WHERE so.statusid IN ($P{ckEstimate}, $P{ckIssued}, $P{ckInProgress}, $P{ckFulfilled}, $P{ckClosedShort}, $P{ckVoided}, $P{ckExpired},$P{ckCancelled})
    AND $P!{cbApplyDateFilter} BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)

),

so_audit_sofu AS (
    SELECT
        so_aud.id,
        so_aud.rev,
        so_aud.username,
        CustomFieldByName(so_aud.customfields, 'SO Follow Up') AS sofu,
        LAG(CustomFieldByName(so_aud.customfields, 'SO Follow Up'))
            OVER (PARTITION BY so_aud.id ORDER BY so_aud.rev) AS prev_sofu
    FROM so_aud
    INNER JOIN so ON so.id = so_aud.id
WHERE so.statusid IN ($P{ckEstimate}, $P{ckIssued}, $P{ckInProgress}, $P{ckFulfilled}, $P{ckClosedShort}, $P{ckVoided}, $P{ckExpired},$P{ckCancelled})
    AND $P!{cbApplyDateFilter} BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)

),

so_audit_ffu AS (
    SELECT
        so_aud.id,
        so_aud.rev,
        so_aud.username,
        CustomFieldByName(so_aud.customfields, 'Fulfilled Follow Up') AS ffu,
        LAG(CustomFieldByName(so_aud.customfields, 'Fulfilled Follow Up'))
            OVER (PARTITION BY so_aud.id ORDER BY so_aud.rev) AS prev_ffu
    FROM so_aud
    INNER JOIN so ON so.id = so_aud.id
WHERE so.statusid IN ($P{ckEstimate}, $P{ckIssued}, $P{ckInProgress}, $P{ckFulfilled}, $P{ckClosedShort}, $P{ckVoided}, $P{ckExpired},$P{ckCancelled})
    AND $P!{cbApplyDateFilter} BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)

),
so_audit_afu AS (
    SELECT
        so_aud.id,
        so_aud.rev,
        so_aud.username,
        CustomFieldByName(so_aud.customfields, 'Ageing Follow Up') AS afu,
        LAG(CustomFieldByName(so_aud.customfields, 'Ageing Follow Up'))
            OVER (PARTITION BY so_aud.id ORDER BY so_aud.rev) AS prev_afu
    FROM so_aud
    INNER JOIN so ON so.id = so_aud.id
WHERE so.statusid IN ($P{ckEstimate}, $P{ckIssued}, $P{ckInProgress}, $P{ckFulfilled}, $P{ckClosedShort}, $P{ckVoided}, $P{ckExpired},$P{ckCancelled})
    AND $P!{cbApplyDateFilter} BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)

)

SELECT

sysuser.username AS USERNAME,
coalesce(ecount,0) as ecount,
coalesce(socount,0) as socount,
coalesce(acount,0) as acount,
coalesce(fcount,0) as fcount,
(coalesce(ecount,0) + coalesce(socount,0) + coalesce(acount,0) + coalesce(fcount,0) )  as total,
locationGroup.name as DEFAULTLG
FROM sysuser
LEFT OUTER JOIN

(SElECT
        username,
        count(*) as ecount
		FROM so_audit_efu
where
	(efu Is Not null
    And prev_efu IS NULL) OR efu <> prev_efu OR (efu Is null
    And prev_efu IS Not NULL
    )
GROUP BY username) AS efu_audit ON sysuser.username = efu_audit.username

LEFT OUTER JOIN

(SElECT
        username,
        count(*) as socount
		FROM so_audit_sofu
where
	(sofu Is Not null
    And prev_sofu IS NULL) OR sofu <> prev_sofu OR (sofu Is null
    And prev_sofu IS Not NULL
    )
GROUP BY username) AS sofu_audit ON sysuser.username = sofu_audit.username


LEFT OUTER JOIN

(SElECT
        username,
        count(*) as acount
		FROM so_audit_afu
where
	(afu Is Not null
    And prev_afu IS NULL) OR afu <> prev_afu OR (afu Is null
    And prev_afu IS Not NULL
    )
GROUP BY username) AS afu_audit ON sysuser.username = afu_audit.username


LEFT OUTER JOIN

(SElECT
        username,
        count(*) as fcount
		FROM so_audit_ffu
where
	(ffu Is Not null
    And prev_ffu IS NULL) OR ffu <> prev_ffu OR (ffu Is null
    And prev_ffu IS Not NULL
    )
GROUP BY username) AS ffu_audit ON sysuser.username = ffu_audit.username

LEFT OUTER JOIN usertolg ON usertolg.userid = sysuser.id AND usertolg.defaultFlag = 1

LEFT OUTER JOIN locationGroup ON  locationGroup.id = usertolg.locationGroupid

WHERE UPPER(sysuser.username) LIKE UPPER($P{salesPerson})
ORDER BY total desc, sysuser.username;]]>
