	<queryString>
		<![CDATA[SELECT
 locationgroup.name AS LG,
 sm.SALESMANAGER,
 company.name AS COMPANY,
 currency.SYMBOL,
 sales1.SALES AS SALES1,
 sales2.SALES AS SALES2,
 sales1.SALES - sales2.SALES AS CNG,
 (sales1.SALES - sales2.SALES)/sales2.SALES AS PERCENTAGE
FROM company
INNER JOIN locationgroup ON 1 = 1
LEFT OUTER JOIN currency ON currency.homeCurrency = 1
LEFT OUTER JOIN
(
 SELECT
	DISTINCT CustomFieldByName(sysuser.customfields, 'Sales Manager') AS SALESMANAGER
    FROM sysuser
) AS sm ON 1=1

-- Sales 1
LEFT OUTER JOIN (
     SELECT
 	so.locationgroupid AS locationgroupid,
          CustomFieldByName(sysuser.customfields, 'Sales Manager') AS SALESMANAGER,
	SUM(SOItem.qtyFulfilled * (SOItem.unitPrice + (soitem.adjustamount / (CASE WHEN soitem.qtytofulfill = 0
                                                            THEN 1
                                                            ELSE soitem.qtytofulfill END)))) AS SALES
    FROM SOItem
    JOIN SO ON SO.ID = SOItem.SOID
    JOIN SysUser ON sysuser.userName = SO.salesman

WHERE soitem.datelastfulfillment BETWEEN $P{dateRange1GrossSales1} AND DATE_SUB($P{dateRange2GrossSales1}, INTERVAL 1 SECOND)
  AND soitem.qtyfulfilled > 0
  AND soitem.typeid IN (10,11,12,$P{ckIncludeReturns},21,60,80)

GROUP BY so.locationgroupid, SALESMANAGER) AS sales1 ON sales1.SALESMANAGER = sm.SALESMANAGER AND locationgroup.id = sales1.locationgroupid

-- Sales 2
LEFT OUTER JOIN (
     SELECT
          so.locationgroupid AS locationgroupid,
          CustomFieldByName(sysuser.customfields, 'Sales Manager') AS SALESMANAGER,
	SUM(SOItem.qtyFulfilled * (SOItem.unitPrice + (soitem.adjustamount / (CASE WHEN soitem.qtytofulfill = 0
                                                            THEN 1
                                                            ELSE soitem.qtytofulfill END)))) AS SALES
    FROM SOItem
    JOIN SO ON SO.ID = SOItem.SOID
    JOIN SysUser ON sysuser.userName = SO.salesman

WHERE soitem.datelastfulfillment BETWEEN $P{dateRange1GrossSales2} AND DATE_SUB($P{dateRange2GrossSales2}, INTERVAL 1 SECOND)
  AND soitem.qtyfulfilled > 0
  AND soitem.typeid IN (10,11,12,$P{ckIncludeReturns},21,60,80)

GROUP BY so.locationgroupid, SALESMANAGER) AS sales2 ON sales2.SALESMANAGER = sm.SALESMANAGER AND locationgroup.id = sales2.locationgroupid

WHERE (sales1.sales > 0 OR sales2.sales>0)
AND locationgroup.id IN ($P!{locationGroupID})

ORDER BY 1, 2;]]>
	</queryString>
