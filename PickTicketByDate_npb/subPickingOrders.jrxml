	<queryString>
		<![CDATA[SELECT DISTINCT PICKITEM.ORDERID AS ORDERID, PICKITEM.ORDERTYPEID AS ORDERTYPEID, ORDERTYPE.NAME AS ORDERTYPE,
    PO.NUM AS PONUM, PO.VENDORCONTACT AS POEXTRA, PO.NOTE AS PONOTE, PO.VENDORSO AS POVENDORSO,
    SO.NUM AS SONUM, SO.CUSTOMERCONTACT AS SOEXTRA, SO.NOTE AS SONOTE, SO.CUSTOMERPO AS SOCUSTOMERPO, SOCUSTOMER.NAME AS SOCUSTOMER,
    WO.NUM AS WONUM, COALESCE(WOCUSTOMER.NAME, '') AS WOEXTRA, WO.NOTE AS WONOTE,
    XO.NUM AS TONUM, CONCAT(FROMLG.NAME, ' -> ', TOLG.NAME) AS TOEXTRA, XO.NOTE AS TONOTE,
    (CASE WHEN PICKITEM.ORDERTYPEID = 10 THEN PO.REMITADDRESS
	  WHEN PICKITEM.ORDERTYPEID = 20 THEN SO.SHIPTOADDRESS
          WHEN PICKITEM.ORDERTYPEID = 40 THEN XO.SHIPTOADDRESS
     END) AS SHIPTOADDRESS,
    (CASE WHEN PICKITEM.ORDERTYPEID = 10 THEN PO.REMITCITY
	  WHEN PICKITEM.ORDERTYPEID = 20 THEN SO.SHIPTOCITY
          WHEN PICKITEM.ORDERTYPEID = 40 THEN XO.SHIPTOCITY
     END) AS SHIPTOCITY,
    (CASE WHEN PICKITEM.ORDERTYPEID = 10 THEN PO.REMITZIP
	  WHEN PICKITEM.ORDERTYPEID = 20 THEN SO.SHIPTOZIP
          WHEN PICKITEM.ORDERTYPEID = 40 THEN XO.SHIPTOZIP
     END) AS SHIPTOZIP,
    (CASE WHEN PICKITEM.ORDERTYPEID = 10 THEN POSTATE.CODE
	  WHEN PICKITEM.ORDERTYPEID = 20 THEN SOSTATE.CODE
          WHEN PICKITEM.ORDERTYPEID = 40 THEN XOSTATE.CODE
     END) AS SHIPSTATE,
    (CASE WHEN PICKITEM.ORDERTYPEID = 10 THEN POCOUNTRY.NAME
	  WHEN PICKITEM.ORDERTYPEID = 20 THEN SOCOUNTRY.NAME
          WHEN PICKITEM.ORDERTYPEID = 40 THEN XOCOUNTRY.NAME
     END) AS SHIPCOUNTRY, PICKITEM.SLOTNUM AS PICKSLOTNUM

FROM pickitem
    INNER JOIN ordertype ON (pickitem.ordertypeid = ordertype.id)
    LEFT OUTER JOIN po ON (pickitem.orderid = po.id AND pickitem.ordertypeid = 10)
    LEFT OUTER JOIN so ON (pickitem.orderid = so.id AND pickitem.ordertypeid = 20)
    LEFT OUTER JOIN wo ON (pickitem.orderid = wo.id AND pickitem.ordertypeid = 30)
    LEFT OUTER JOIN xo ON (pickitem.orderid = xo.id AND pickitem.ordertypeid = 40)
    LEFT OUTER JOIN locationgroup AS fromlg ON (xo.fromlgid = fromlg.id)
    LEFT OUTER JOIN locationgroup AS tolg ON (xo.shiptolgid = tolg.id)
    LEFT JOIN stateconst AS postate ON po.remitstateid = postate.id
    LEFT JOIN stateconst AS sostate ON so.shiptostateid = sostate.id
    LEFT JOIN stateconst AS xostate ON xo.shiptostateid = xostate.id
    LEFT JOIN countryconst AS pocountry ON po.remitcountryid = pocountry.id
    LEFT JOIN countryconst AS socountry ON so.shiptocountryid = socountry.id
    LEFT JOIN countryconst AS xocountry ON xo.shiptocountryid = xocountry.id
    LEFT JOIN customer AS wocustomer ON wo.customerid = wocustomer.id
    LEFT JOIN customer AS socustomer ON so.customerid = socustomer.id

WHERE pickitem.pickid = $P{pickID}]]>
	</queryString>
