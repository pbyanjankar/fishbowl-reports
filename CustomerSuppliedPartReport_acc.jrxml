		</parameter>
		<queryString>
			<![CDATA[SELECT
RECEIPTITEM.ID AS RECEIPTITEMID,
PO.NUM AS PONUM,
receiptitem.qty AS QTY,
RECEIPTITEM.DATERECEIVED AS RECEIPTITEMDATERECEIVED,
trackinginfo.info AS LOTNUMBER
FROM receiptitem
    JOIN receipt ON receipt.id = receiptitem.receiptid
    JOIN poitem ON poitem.id = receiptitem.poitemid
    JOIN po ON po.id = poitem.poid
    LEFT JOIN part ON part.id = poitem.partid
    LEFT OUTER JOIN trackinginfo ON tableid = 1259138490 AND receiptitem.id = trackinginfo.recordid AND trackinginfo.parttrackingid = 1
WHERE receiptitem.datereceived BETWEEN $P{dateRange1} AND DATE_SUB($P{dateRange2}, INTERVAL 1 SECOND)
  AND UPPER(part.num) LIKE UPPER($P{partNum})
  AND receiptitem.typeid = 10
  AND Receipt.ordertypeid = 10;]]>
