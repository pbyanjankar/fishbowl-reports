	<queryString>
		<![CDATA[SELECT DISTINCT (CASE WHEN ROPLG.SYSVALUE = 'TRUE' THEN LOCATIONGROUP.NAME ELSE PART.NUM END) AS ORDERBY,
    PART.NUM AS PART, COALESCE(COMPANYWIDERP.ORDERUPTOLEVEL, PARTREORDER.ORDERUPTOLEVEL,0) AS ORDERUPTOLEVEL,
    COALESCE(PARTREORDER.REORDERPOINT, COMPANYWIDERP.REORDERPOINT,0) AS REORDERPOINT, UOM.CODE AS UOMCODE,
    COALESCE(LOCATIONGROUP.NAME,'COMPANY WIDE') AS LGNAME, COALESCE(QTYONHAND.QTY,0) AS QTY,
    COALESCE(QTYALLOCATED.QTY,0) AS ALLOCATED, COALESCE(QTYONORDER.QTY,0) AS ONORDER,
    COALESCE(QTYNOTAVAILABLE.QTY,0) AS NOTAVAILABLE, COALESCE(QTYDROPSHIP.QTY,0) AS DROPSHIP,
    COALESCE(TOTALS.QTYONHAND, 0) AS TOTALQTY, COALESCE(TOTALS.QTYALLOCATED, 0) AS TOTALALLOCATED,
    COALESCE(TOTALS.QTYONORDER, 0) AS TOTALONORDER, COALESCE(TOTALS.QTYNOTAVAILABLE,0) AS TOTALNOTAVAILABLE,
    COALESCE(TOTALS.QTYDROPSHIP,0) AS TOTALDROPSHIP,
    COALESCE((SELECT COALESCE(PARTREORDER.REORDERPOINT,0) AS QTY FROM PARTREORDER WHERE PARTREORDER.PARTID = PART.ID AND PARTREORDER.LOCATIONGROUPID IS NULL),0) AS COMPANYREORDERPOINT,
    COALESCE((SELECT COALESCE(PARTREORDER.ORDERUPTOLEVEL,0) AS QTY FROM PARTREORDER WHERE PARTREORDER.PARTID = PART.ID AND PARTREORDER.LOCATIONGROUPID IS NULL),0) AS COMPANYORDERUPTOLEVEL,
    COMPANY.NAME AS COMPANY, PARTREORDER.LOCATIONGROUPID, PART.DESCRIPTION, VENDOR.NAME AS VENDOR,
    COALESCE(ROPLG.SYSVALUE, 'false') AS REORDERBYLOCATIONGROUP, part.abccode

FROM part
    LEFT JOIN (SELECT SYSPROPERTIES.sysvalue AS sysvalue FROM SYSPROPERTIES WHERE syskey = 'locationGroupReordering') ropLG ON (1=1)
    LEFT JOIN locationgroup ON locationgroup.id IN ($P!{locationGroupID})
    LEFT JOIN partreorder ON locationgroup.id = partreorder.locationgroupid AND part.id = partreorder.partid
    LEFT JOIN partreorder CompanyWideRP on part.id = CompanyWideRP.partid AND COALESCE(ropLG.sysvalue,'false') = 'false'
    LEFT JOIN uom ON part.uomid = uom.id
    LEFT JOIN qtyonhand ON (part.id = qtyonhand.partid AND locationgroup.id = qtyonhand.locationgroupid AND qtyonhand.locationgroupid=1)
    LEFT JOIN qtyallocated ON (part.id = qtyallocated.partid AND locationgroup.id = qtyallocated.locationgroupid AND qtyallocated.locationgroupid=1)
    LEFT JOIN qtyonorder ON (part.id = qtyonorder.partid AND locationgroup.id = qtyonorder.locationgroupid AND qtyonorder.locationgroupid=1)
    LEFT JOIN qtynotavailable ON (part.id = qtynotavailable.partid AND locationgroup.id = qtynotavailable.locationgroupid AND qtynotavailable.locationgroupid=1)
    LEFT JOIN qtydropship ON (part.id = qtydropship.partid AND locationgroup.id = qtydropship.locationgroupid AND qtydropship.locationgroupid=1)
    LEFT JOIN (SELECT COALESCE(SUM(qtyinventory.qtyonhand),0) AS qtyonhand,
                   COALESCE(SUM((qtyinventory.qtyallocatedpo + qtyinventory.qtyallocatedso + qtyinventory.qtyallocatedto + qtyinventory.qtyallocatedmo)),0) AS qtyallocated,
                   COALESCE(SUM((qtyinventory.qtyonorderpo + qtyinventory.qtyonorderso + qtyinventory.qtyonorderto + qtyinventory.qtyonordermo)),0) AS qtyonorder,
                   COALESCE(SUM(qtyinventory.qtynotavailable),0) AS qtynotavailable,
                   COALESCE(SUM(qtyinventory.qtydropship),0) AS qtydropship, qtyinventory.partid
               FROM qtyinventory
	     WHERE qtyinventory.locationgroupid = 1
               GROUP BY qtyinventory.partid) AS totals ON part.id = totals.partid
    INNER JOIN company ON company.id = 1
    LEFT JOIN vendorparts ON part.id = vendorparts.partid AND vendorparts.defaultflag = 1
    LEFT JOIN vendor ON vendorparts.vendorid = vendor.id
    LEFT JOIN (SELECT partid
	FROM bomitem
	WHERE bomitem.typeid = 10
	) onbom ON onbom.partid = part.id

WHERE part.id != 0
  AND (LOCATIONGROUP.ACTIVEFLAG IS TRUE OR PARTREORDER.REORDERPOINT > 0)
  AND part.typeid = 10
  AND part.num LIKE $P{partNum}
  AND part.activeflag IN (1,$P!{ckShowInactiveParts})
  AND (partreorder.locationgroupid IN ($P!{locationGroupID}) OR partreorder.locationgroupid IS NULL)
  AND (($P{ShowPartsWithBOM} = true AND COALESCE(ONBOM.PARTID,'NK5578HHLACA') != 'NK5578HHLACA')
	 OR ($P{ShowPartsWithoutBOM} = true AND COALESCE(ONBOM.PARTID,'NK5578HHLACA') = 'NK5578HHLACA'))

ORDER BY 1, 2]]>
	</queryString>
