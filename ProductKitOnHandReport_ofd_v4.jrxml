		<queryString>
			<![CDATA[SELECT
   mproduct.NUM AS PRODUCTNAME,
   mproduct.DESCRIPTION AS PRODUCTDESCRIPTION,
   mproduct.kitFlag AS KITFLAG,
   mproduct.price AS PRODUCTPRICE,
   mproduct.weight AS PRODUCTWEIGHT,
   mproductweightuom.code AS PRODUCTWEIGHTUOM,
   CURRENCY.SYMBOL,
   if(KitProductTotals.kitAvailableQty<0, 0, KitProductTotals.kitAvailableQty)  AS KITAVAILABLEQTY,
   KitProductTotals.kitOrderQty AS KITORDERQTY,
   closestPO.closestScheduledDate AS CLOSESTSCHEDULEDDATE,
   kitProductTotals.KitVendorCost AS KITVENDORCOST,
   kitProductTotals.KitProductPrice AS KITPRODUCTPRICE,
   kitProductTotals.KitWeight AS KITWEIGHT,
   weightuom.code AS WEIGHTUOM,
   mproduct.upc AS UPC,
   if(inventoryTotals.totalOnHand - inventoryTotals.totalNotAvailable-inventoryTotals.totalAllocated+inventoryTotals.totaldropShip <0, 0, inventoryTotals.totalOnHand - inventoryTotals.totalNotAvailable-inventoryTotals.totalAllocated+inventoryTotals.totaldropShip )  AS PRODUCTAVAILABLEQTY,
   inventoryTotals.totalOnOrder AS PRODUCTORDERQTY,
   CASE '$P!{cbCostingMethod}'
      WHEN 'Average Cost' THEN COALESCE(mpartcost.AVGCOST, 0)
      WHEN 'FIFO Cost' THEN COALESCE((SELECT ORGTOTALCOST / ORGQTY FROM COSTLAYER WHERE PARTID = mpart.ID AND STATUSID = 10 ORDER BY ID ASC LIMIT 1), 0)
      WHEN 'LIFO Cost' THEN COALESCE((SELECT ORGTOTALCOST / ORGQTY FROM COSTLAYER WHERE PARTID = mpart.ID AND STATUSID = 10 ORDER BY ID DESC LIMIT 1), 0)
      WHEN 'Standard Cost' THEN COALESCE(mpart.STDCOST, 0)
      ELSE 0
      END AS PRODUCTUNITCOST
FROM product AS mproduct
   LEFT JOIN (
	SELECT product.id AS productId,
		min(coalesce(kitInventoryTotals.totalOnHand - totalNotAvailable-totalAllocated+totaldropShip,0)/defaultQty) AS KitAvailableQty,
		min(coalesce(kitInventoryTotals.totalOnOrder,0)/defaultQty) AS KitOrderQty,
  		sum(defaultQty
			*
		   CASE '$P!{cbCostingMethod}'
      		     WHEN 'Average Cost' THEN COALESCE(partcost.AVGCOST, 0)
     		     WHEN 'FIFO Cost' THEN COALESCE((SELECT ORGTOTALCOST / ORGQTY FROM COSTLAYER WHERE PARTID =part.ID AND STATUSID = 10 ORDER BY ID ASC LIMIT 1), 0)
      		     WHEN 'LIFO Cost' THEN COALESCE((SELECT ORGTOTALCOST / ORGQTY FROM COSTLAYER WHERE PARTID = part.ID AND STATUSID = 10 ORDER BY ID DESC LIMIT 1), 0)
     		     WHEN 'Standard Cost' THEN COALESCE(part.STDCOST, 0)
      	    	     ELSE 0
     		 END

		) AS KitVendorCost,
 		sum(defaultQty*p2.price) AS KitProductPrice,
 		sum(defaultQty*p2.weight*COALESCE(UOMCONVERSION.FACTOR,1)*COALESCE(UOMCONVERSION.MULTIPLY,1)) AS KitWeight
	FROM kititem
   	 INNER JOIN product ON product.id = kititem.kitProductId
   	 INNER JOIN product p2 ON p2.id = kititem.productId
   	 INNER JOIN part ON p2.partid = part.id
           LEFT JOIN partcost ON p2.partid = partcost.partId
           LEFT JOIN uomconversion ON (uomconversion.touomid = 3 AND uomconversion.fromuomid = part.weightUomId)
	 LEFT JOIN  vendorparts ON part.id = vendorparts.partId AND vendorparts.defaultFlag=true
	 LEFT JOIN (
        	SELECT
            partId,
            SUM(qtyOnHand) AS totalOnHand,
            SUM(qtyNotAvailable) AS totalNotAvailable,
            SUM(qtyNotAvailabletopick) AS totalNotAvailabletopick,
            SUM(qtyDropship) AS totalDropship,
            SUM(qtyAllocated) AS totalAllocated,
            SUM(qtyOnOrder) AS totalOnOrder
        FROM
            qtyInventoryTotals
        GROUP BY partId
   	 ) AS kitInventoryTotals ON kitInventoryTotals.partId = part.id
	group by product.id
   ) AS KitProductTotals ON KitProductTotals.productId = mproduct.id
   LEFT JOIN currency ON currency.homeCurrency = 1
   LEFT JOIN (
	 SELECT product.id AS productId,
	        min(poitem.datescheduledfulfillment) AS closestScheduledDate
	 FROM product
	 INNER JOIN part ON part.id = product.partId
	 INNER JOIN poitem ON poitem.partId = part.id
           INNER JOIN po ON po.id = poitem.poid
	 WHERE poitem.statusId IN (10,20,30,40,45) AND po.statusId IN (20,30,40,45,50,55)
	 GROUP BY product.id
	) AS ClosestPO ON ClosestPO.productId = mproduct.Id
   LEFT JOIN uom AS weightuom ON mproduct.weightuomid = weightuom.id
   -- Non Kit Product
   LEFT JOIN part AS mpart ON mpart.id = mproduct.partId
   LEFT JOIN partcost AS mpartcost ON mpartcost.partid = mpart.id
   LEFT JOIN (SELECT
            partId,
            SUM(qtyOnHand) AS totalOnHand,
            SUM(qtyNotAvailable) AS totalNotAvailable,
            SUM(qtyNotAvailabletopick) AS totalNotAvailabletopick,
            SUM(qtyDropship) AS totalDropship,
            SUM(qtyAllocated) AS totalAllocated,
            SUM(qtyOnOrder) AS totalOnOrder
        FROM
            qtyInventoryTotals
        GROUP BY partId
   	 ) AS inventoryTotals ON inventoryTotals.partId = mpart.id
    LEFT JOIN uom AS mproductweightuom ON mproductweightuom.id = mproduct.weightuomid


WHERE
  mproduct.activeflag = 1
  AND mproduct.id != 0
  AND UPPER(mproduct.num) LIKE UPPER($P{productNum})
  AND (if($P{AmazonPlatform},CustomFieldByName(mproduct.customfields,'Amazon Platform') = "true",false)  OR
if($P{NationalBusinessFurniture},CustomFieldByName(mproduct.customfields,'National Business Furniture') = "true",false) OR
if($P{WalmartPlatform},CustomFieldByName(mproduct.customfields,'Walmart Platform') = "true",false) OR
if($P{WayfairPlatform},CustomFieldByName(mproduct.customfields,'Wayfair Platform') = "true",false) OR
if($P{WorthingtonDirect},CustomFieldByName(mproduct.customfields,'Worthington Direct') = "true",false) OR
if($P{ZoroPlatform},CustomFieldByName(mproduct.customfields,'Zoro Platform') = "true",false) OR
if($P{FizzFurniture},CustomFieldByName(mproduct.customfields,'Fizz Furniture') = "true",false) OR
if($P{Micro-Site},CustomFieldByName(mproduct.customfields,'Micro-Site') = "true",false) OR
if($P{WalmartItems},CustomFieldByName(mproduct.customfields,'Walmart Items') = "true",false) OR
if($P{OFDSiteItems},CustomFieldByName(mproduct.customfields,'OFD Site Items') = "true",false) OR
if($P{NewPlatform6},CustomFieldByName(mproduct.customfields,'New Platform 6') = "true",false) OR
if($P{NewPlatform7},CustomFieldByName(mproduct.customfields,'New Platform 7') = "true",false) OR
if($P{NewPlatform8},CustomFieldByName(mproduct.customfields,'New Platform 8') = "true",false) OR
if($P{NewPlatform9},CustomFieldByName(mproduct.customfields,'New Platform 9') = "true",false) OR
if($P{NewPlatform10},CustomFieldByName(mproduct.customfields,'New Platform 10') = "true",false) OR
if($P{NewPlatform11},CustomFieldByName(mproduct.customfields,'New Platform 11') = "true",false) OR
if($P{NewPlatform12},CustomFieldByName(mproduct.customfields,'New Platform 12') = "true",false) OR
if($P{NewPlatform13},CustomFieldByName(mproduct.customfields,'New Platform 13') = "true",false) OR
if($P{NewPlatform14},CustomFieldByName(mproduct.customfields,'New Platform 14') = "true",false) OR
if($P{NewPlatform15},CustomFieldByName(mproduct.customfields,'New Platform 15') = "true",false)
)

ORDER BY mproduct.num;]]>
		</queryString>
